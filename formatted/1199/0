<span class="c">// example</span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs87', 258)" onmouseover="showTip(event, 'fs87', 258)" class="f">test</span> () <span class="o">=</span> 
    <span onmouseout="hideTip(event, 'fs34', 259)" onmouseover="showTip(event, 'fs34', 259)" class="i">eff</span> {
        <span class="k">let!</span> <span onmouseout="hideTip(event, 'fs80', 260)" onmouseover="showTip(event, 'fs80', 260)" class="i">x</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs56', 261)" onmouseover="showTip(event, 'fs56', 261)" class="f">get</span> ()
