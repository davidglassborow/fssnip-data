				<span class="i">out</span><span class="o">.</span><span class="i">today</span><span class="o">.</span><span class="i">wind</span><span class="o">.</span><span class="i">deg</span> <span class="o">=</span> <span class="i">input</span><span class="o">.</span><span class="i">wind</span><span class="o">.</span><span class="i">direction</span>;
