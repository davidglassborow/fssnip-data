						<span onmouseout="hideTip(event, 'fs1', 17)" onmouseover="showTip(event, 'fs1', 17)" class="i">min</span> <span class="o">:</span> <span class="i">Number</span>, <span class="i">ex</span><span class="o">.</span> <span class="n">24</span>
						<span onmouseout="hideTip(event, 'fs5', 18)" onmouseover="showTip(event, 'fs5', 18)" class="i">max</span> <span class="o">:</span> <span class="i">Number</span> <span class="i">ex</span><span class="o">.</span> <span class="n">12</span>
					},
					<span class="i">desc</span> <span class="o">:</span> <span onmouseout="hideTip(event, 'fs6', 19)" onmouseover="showTip(event, 'fs6', 19)" class="i">String</span>, <span class="i">ex</span><span class="o">.</span> <span class="s">&quot;Partly Cloudy&quot;</span>
					<span class="i">code</span> <span class="o">:</span> <span class="i">Number</span>, <span class="i">ex</span><span class="o">.</span> <span class="s">&quot;801&quot;</span> <span class="i">see</span> <span class="i">css</span> <span class="k">or</span> <span class="i">weather</span> <span class="i">codes</span> <span class="k">for</span> <span class="i">meaning</span>
					<span class="i">wind</span> <span class="o">:</span> {
						<span class="i">speed</span> <span class="o">:</span> <span class="n">4</span>, 
						<span class="i">deg</span> <span class="o">:</span> <span class="i">Number</span>, 
					},
					<span class="i">pressure</span> <span class="o">:</span> <span class="i">Number</span>,
					<span class="i">humidity</span> <span class="o">:</span> <span class="i">Number</span>, 
					<span class="i">sunrise</span> <span class="o">:</span> <span class="i">Time</span>,
					<span class="i">sunset</span> <span class="o">:</span> <span class="i">Time</span>,
					<span class="i">day</span> <span class="o">:</span>  <span onmouseout="hideTip(event, 'fs6', 20)" onmouseover="showTip(event, 'fs6', 20)" class="i">String</span>,

				},
				<span class="i">forecast</span> <span class="o">:</span> [{<span class="i">Day</span><span class="o">:</span> <span onmouseout="hideTip(event, 'fs6', 21)" onmouseover="showTip(event, 'fs6', 21)" class="i">String</span>, <span class="i">code</span><span class="o">:</span><span class="i">Number</span>, <span class="i">desc</span><span class="o">:</span> <span onmouseout="hideTip(event, 'fs6', 22)" onmouseover="showTip(event, 'fs6', 22)" class="i">String</span>, <span class="i">temp</span> <span class="o">:</span> {<span onmouseout="hideTip(event, 'fs1', 23)" onmouseover="showTip(event, 'fs1', 23)" class="i">min</span><span class="o">:</span><span class="i">number</span>, <span onmouseout="hideTip(event, 'fs5', 24)" onmouseover="showTip(event, 'fs5', 24)" class="i">max</span><span class="o">:</span><span class="i">number</span>}}]
			}
		<span class="o">*/</span>
		<span class="k">function</span> <span class="i">datamapper</span> (<span class="i">input</span>, <span class="i">settings</span>) {

			<span class="i">var</span> <span class="i">out</span> <span class="o">=</span> {}; <span class="c">//map input to out</span>

			<span class="k">if</span> (<span class="i">settings</span><span class="o">.</span><span class="i">api</span> <span class="o">==</span> <span class="s">&quot;openweathermap&quot;</span>) {

				<span class="c">//data[0] is current weather, data[1] is forecast</span>
				<span class="k">if</span> (<span class="i">input</span>[<span class="n">0</span>]<span class="o">.</span><span class="i">name</span> <span class="o">!=</span> <span class="s">&quot;&quot;</span>) {
					<span class="i">out</span><span class="o">.</span><span class="i">location</span> <span class="o">=</span> <span class="i">input</span>[<span class="n">0</span>]<span class="o">.</span><span class="i">name</span> <span class="o">+</span> <span class="s">&quot;, &quot;</span> <span class="o">+</span> <span class="i">input</span>[<span class="n">0</span>]<span class="o">.</span><span class="i">sys</span><span class="o">.</span><span class="i">country</span>;
					<span class="i">out</span><span class="o">.</span><span class="i">city</span> <span class="o">=</span>  <span class="i">input</span>[<span class="n">0</span>]<span class="o">.</span><span class="i">name</span>;
				}
				<span class="k">else</span> <span class="k">if</span> (<span class="i">input</span>[<span class="n">1</span>]<span class="o">.</span><span class="i">city</span><span class="o">.</span><span class="i">name</span> <span class="o">!=</span> <span class="s">&quot;&quot;</span>){
