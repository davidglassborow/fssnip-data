<span class="c">//Problem 1</span>

<span class="k">type</span> <span class="t">Name</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs1', 1)" onmouseover="showTip(event, 'fs1', 1)" class="t">string</span>
<span class="k">type</span> <span onmouseout="hideTip(event, 'fs2', 2)" onmouseover="showTip(event, 'fs2', 2)" class="t">Event</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs1', 3)" onmouseover="showTip(event, 'fs1', 3)" class="t">string</span>
<span class="k">type</span> <span onmouseout="hideTip(event, 'fs3', 4)" onmouseover="showTip(event, 'fs3', 4)" class="t">Point</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs4', 5)" onmouseover="showTip(event, 'fs4', 5)" class="t">int</span>
<span class="k">type</span> <span onmouseout="hideTip(event, 'fs5', 6)" onmouseover="showTip(event, 'fs5', 6)" class="t">Score</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs6', 7)" onmouseover="showTip(event, 'fs6', 7)" class="t">Name</span> <span class="o">*</span> <span onmouseout="hideTip(event, 'fs2', 8)" onmouseover="showTip(event, 'fs2', 8)" class="t">Event</span> <span class="o">*</span> <span onmouseout="hideTip(event, 'fs3', 9)" onmouseover="showTip(event, 'fs3', 9)" class="t">Point</span>
   
<span class="k">type</span> <span onmouseout="hideTip(event, 'fs7', 10)" onmouseover="showTip(event, 'fs7', 10)" class="t">Scoreboard</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs5', 11)" onmouseover="showTip(event, 'fs5', 11)" class="t">Score</span> <span onmouseout="hideTip(event, 'fs8', 12)" onmouseover="showTip(event, 'fs8', 12)" class="t">list</span>
   
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs9', 13)" onmouseover="showTip(event, 'fs9', 13)" class="i">sb</span> <span class="o">=</span> [(<span class="s">&quot;Joe&quot;</span>, <span class="s">&quot;June Fishing&quot;</span>, <span class="n">35</span>); (<span class="s">&quot;Peter&quot;</span>, <span class="s">&quot;May Fishing&quot;</span>, <span class="n">30</span>);
          (<span class="s">&quot;Joe&quot;</span>, <span class="s">&quot;May Fishing&quot;</span>, <span class="n">28</span>); (<span class="s">&quot;Paul&quot;</span>, <span class="s">&quot;June Fishing&quot;</span>, <span class="n">28</span>)];;

<span class="c">//1  inv: Scoreboard -&gt; bool</span>
