        <span class="c">//Take first line as the title for untitled poems</span>
        | <span class="s">&quot;* * *&quot;</span> <span class="k">-&gt;</span> (<span class="k">if</span> (<span onmouseout="hideTip(event, 'fs82', 194)" onmouseover="showTip(event, 'fs82', 194)" class="i">lines</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs37', 195)" onmouseover="showTip(event, 'fs37', 195)" class="t">Seq</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs89', 196)" onmouseover="showTip(event, 'fs89', 196)" class="f">isEmpty</span>) <span class="k">then</span> <span class="s">&quot;&quot;</span> <span class="k">else</span> (<span onmouseout="hideTip(event, 'fs82', 197)" onmouseover="showTip(event, 'fs82', 197)" class="i">lines</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs37', 198)" onmouseover="showTip(event, 'fs37', 198)" class="t">Seq</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs90', 199)" onmouseover="showTip(event, 'fs90', 199)" class="f">nth</span> <span class="n">0</span>))
        | _ <span class="k">-&gt;</span> <span onmouseout="hideTip(event, 'fs81', 200)" onmouseover="showTip(event, 'fs81', 200)" class="i">title</span>

    <span class="k">member</span> <span onmouseout="hideTip(event, 'fs91', 201)" onmouseover="showTip(event, 'fs91', 201)" class="i">this</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs92', 202)" onmouseover="showTip(event, 'fs92', 202)" class="i">Href</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs80', 203)" onmouseover="showTip(event, 'fs80', 203)" class="i">poemHref</span>
