    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs83', 181)" onmouseover="showTip(event, 'fs83', 181)" class="i">parsedLines</span> <span class="o">=</span> [ <span class="k">for</span> <span onmouseout="hideTip(event, 'fs84', 182)" onmouseover="showTip(event, 'fs84', 182)" class="i">line</span> <span class="k">in</span> <span onmouseout="hideTip(event, 'fs82', 183)" onmouseover="showTip(event, 'fs82', 183)" class="i">lines</span> <span class="k">-&gt;</span> <span onmouseout="hideTip(event, 'fs84', 184)" onmouseover="showTip(event, 'fs84', 184)" class="i">line</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs85', 185)" onmouseover="showTip(event, 'fs85', 185)" class="f">Replace</span>(<span class="s">&quot;&amp;nbsp;&quot;</span> , <span class="s">&quot;&quot;</span>) ]
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs86', 186)" onmouseover="showTip(event, 'fs86', 186)" class="i">lineTokens</span> <span class="o">=</span> [ <span class="k">for</span> <span onmouseout="hideTip(event, 'fs84', 187)" onmouseover="showTip(event, 'fs84', 187)" class="i">line</span> <span class="k">in</span> <span onmouseout="hideTip(event, 'fs82', 188)" onmouseover="showTip(event, 'fs82', 188)" class="i">lines</span> <span class="k">-&gt;</span> <span onmouseout="hideTip(event, 'fs31', 189)" onmouseover="showTip(event, 'fs31', 189)" class="f">regexMatches</span> (<span onmouseout="hideTip(event, 'fs84', 190)" onmouseover="showTip(event, 'fs84', 190)" class="i">line</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs87', 191)" onmouseover="showTip(event, 'fs87', 191)" class="f">ToLower</span>()) <span class="s">&quot;([а-я]+)&quot;</span> ]
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs88', 192)" onmouseover="showTip(event, 'fs88', 192)" class="i">parsedTitle</span> <span class="o">=</span> 
