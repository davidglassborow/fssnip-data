<span class="c">// a list of floats </span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs1', 1)" onmouseover="showTip(event, 'fs1', 1)" class="i">list1</span> <span class="o">=</span> [<span class="n">1.0</span>; <span class="n">2.0</span>; <span class="n">5.0</span>];;

<span class="c">// Make a list of integers from 1 to 4</span>
