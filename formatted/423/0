<span class="c">// Making a list of floats from 1.0 to 3.0, with step size 0.2</span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs3', 3)" onmouseover="showTip(event, 'fs3', 3)" class="i">list3</span> <span class="o">=</span> [<span class="n">1.0</span> <span class="o">..</span> <span class="n">0.2</span> <span class="o">..</span> <span class="n">3.0</span>];;

<span class="c">// map allows you to aply a function to each element in a list</span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs4', 4)" onmouseover="showTip(event, 'fs4', 4)" class="i">list4</span> <span class="o">=</span> 
    <span onmouseout="hideTip(event, 'fs5', 5)" onmouseover="showTip(event, 'fs5', 5)" class="t">List</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs6', 6)" onmouseover="showTip(event, 'fs6', 6)" class="f">map</span> (<span class="k">fun</span> <span onmouseout="hideTip(event, 'fs7', 7)" onmouseover="showTip(event, 'fs7', 7)" class="i">x</span> <span class="k">-&gt;</span> <span class="n">2</span> <span class="o">*</span> <span onmouseout="hideTip(event, 'fs7', 8)" onmouseover="showTip(event, 'fs7', 8)" class="i">x</span>) <span onmouseout="hideTip(event, 'fs2', 9)" onmouseover="showTip(event, 'fs2', 9)" class="i">list2</span>;;

<span class="c">// Shortcut to pi</span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs8', 10)" onmouseover="showTip(event, 'fs8', 10)" class="i">pi</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs9', 11)" onmouseover="showTip(event, 'fs9', 11)" class="f">atan2</span> <span class="n">0.0</span> <span class="o">-</span><span class="n">1.0</span> 
    
<span class="c">// Make the list of fractions for roots on p.14</span>
<span class="c">// So we want </span>
<span class="c">//    theta + 2 * k * pi / n where k = 0... n-1</span>
<span class="c">// Now we will take angles mod 2 * pi, so they&#39;re in the range [0...2 * pi)</span>
<span class="c">// And sort them from small to large.</span>
