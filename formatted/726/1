    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs16', 188)" onmouseover="showTip(event, 'fs16', 188)" class="i">a</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs58', 189)" onmouseover="showTip(event, 'fs58', 189)" class="i">v1</span> <span class="o">/</span> <span onmouseout="hideTip(event, 'fs61', 190)" onmouseover="showTip(event, 'fs61', 190)" class="i">v2</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs17', 191)" onmouseover="showTip(event, 'fs17', 191)" class="i">b</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs52', 192)" onmouseover="showTip(event, 'fs52', 192)" class="i">yavg</span> <span class="o">-</span> <span onmouseout="hideTip(event, 'fs16', 193)" onmouseover="showTip(event, 'fs16', 193)" class="i">a</span> <span class="o">*</span> <span onmouseout="hideTip(event, 'fs51', 194)" onmouseover="showTip(event, 'fs51', 194)" class="i">xavg</span> 
    <span onmouseout="hideTip(event, 'fs50', 195)" onmouseover="showTip(event, 'fs50', 195)" class="i">values</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs62', 196)" onmouseover="showTip(event, 'fs62', 196)" class="t">Array</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs63', 197)" onmouseover="showTip(event, 'fs63', 197)" class="f">mapi</span> (<span class="k">fun</span> <span onmouseout="hideTip(event, 'fs56', 198)" onmouseover="showTip(event, 'fs56', 198)" class="i">x</span> (<span onmouseout="hideTip(event, 'fs64', 199)" onmouseover="showTip(event, 'fs64', 199)" class="i">dt</span>, _) <span class="k">-&gt;</span> (<span onmouseout="hideTip(event, 'fs64', 200)" onmouseover="showTip(event, 'fs64', 200)" class="i">dt</span>, <span onmouseout="hideTip(event, 'fs16', 201)" onmouseover="showTip(event, 'fs16', 201)" class="i">a</span> <span class="o">*</span> (<span onmouseout="hideTip(event, 'fs10', 202)" onmouseover="showTip(event, 'fs10', 202)" class="f">float</span> <span onmouseout="hideTip(event, 'fs56', 203)" onmouseover="showTip(event, 'fs56', 203)" class="i">x</span>) <span class="o">+</span> <span onmouseout="hideTip(event, 'fs17', 204)" onmouseover="showTip(event, 'fs17', 204)" class="i">b</span>)) <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs49', 205)" onmouseover="showTip(event, 'fs49', 205)" class="f">f</span>)

  <span class="c">/// Runs a classifier and transforms the result using a specified function</span>
  <span class="k">let</span> <span onmouseout="hideTip(event, 'fs65', 206)" onmouseover="showTip(event, 'fs65', 206)" class="f">map</span> <span onmouseout="hideTip(event, 'fs66', 207)" onmouseover="showTip(event, 'fs66', 207)" class="f">g</span> (<span onmouseout="hideTip(event, 'fs8', 208)" onmouseover="showTip(event, 'fs8', 208)" class="p">PT</span> <span onmouseout="hideTip(event, 'fs49', 209)" onmouseover="showTip(event, 'fs49', 209)" class="f">f</span>) <span class="o">=</span> <span onmouseout="hideTip(event, 'fs8', 210)" onmouseover="showTip(event, 'fs8', 210)" class="p">PT</span> (<span onmouseout="hideTip(event, 'fs49', 211)" onmouseover="showTip(event, 'fs49', 211)" class="f">f</span> <span class="o">&gt;</span><span class="o">&gt;</span> <span onmouseout="hideTip(event, 'fs66', 212)" onmouseover="showTip(event, 'fs66', 212)" class="f">g</span>)

  <span class="c">/// Creates a classifier that combines the result of two classifiers using a tuple</span>
  <span class="k">let</span> <span onmouseout="hideTip(event, 'fs67', 213)" onmouseover="showTip(event, 'fs67', 213)" class="f">merge</span> (<span onmouseout="hideTip(event, 'fs8', 214)" onmouseover="showTip(event, 'fs8', 214)" class="p">PT</span> <span onmouseout="hideTip(event, 'fs49', 215)" onmouseover="showTip(event, 'fs49', 215)" class="f">f</span>) (<span onmouseout="hideTip(event, 'fs8', 216)" onmouseover="showTip(event, 'fs8', 216)" class="p">PT</span> <span onmouseout="hideTip(event, 'fs68', 217)" onmouseover="showTip(event, 'fs68', 217)" class="f">g</span>) <span class="o">=</span> <span onmouseout="hideTip(event, 'fs8', 218)" onmouseover="showTip(event, 'fs8', 218)" class="p">PT</span> (<span class="k">fun</span> <span onmouseout="hideTip(event, 'fs50', 219)" onmouseover="showTip(event, 'fs50', 219)" class="i">values</span> <span class="k">-&gt;</span> <span onmouseout="hideTip(event, 'fs49', 220)" onmouseover="showTip(event, 'fs49', 220)" class="f">f</span> <span onmouseout="hideTip(event, 'fs50', 221)" onmouseover="showTip(event, 'fs50', 221)" class="i">values</span>, <span onmouseout="hideTip(event, 'fs68', 222)" onmouseover="showTip(event, 'fs68', 222)" class="f">g</span> <span onmouseout="hideTip(event, 'fs50', 223)" onmouseover="showTip(event, 'fs50', 223)" class="i">values</span>)

<span class="c">/// Does the property hold over the entire data set?</span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs69', 224)" onmouseover="showTip(event, 'fs69', 224)" class="f">run</span> (<span onmouseout="hideTip(event, 'fs8', 225)" onmouseover="showTip(event, 'fs8', 225)" class="p">PT</span> <span onmouseout="hideTip(event, 'fs49', 226)" onmouseover="showTip(event, 'fs49', 226)" class="f">f</span>) (<span onmouseout="hideTip(event, 'fs70', 227)" onmouseover="showTip(event, 'fs70', 227)" class="i">data</span><span class="o">:</span>(<span onmouseout="hideTip(event, 'fs9', 228)" onmouseover="showTip(event, 'fs9', 228)" class="t">DateTime</span> <span class="o">*</span> <span onmouseout="hideTip(event, 'fs10', 229)" onmouseover="showTip(event, 'fs10', 229)" class="t">float</span>)[]) <span class="o">=</span> 
  <span onmouseout="hideTip(event, 'fs49', 230)" onmouseover="showTip(event, 'fs49', 230)" class="f">f</span> <span onmouseout="hideTip(event, 'fs70', 231)" onmouseover="showTip(event, 'fs70', 231)" class="i">data</span>


<span class="c">// ------------------------------------------------------------------</span>
<span class="c">// Downloading &amp; visualizing stock prices from Yahoo</span>


<span class="c">/// Asynchronously downloads stock prices from Yahoo</span>
<span class="c">/// (uses a proxy to enable cross-domain downloads)</span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs71', 232)" onmouseover="showTip(event, 'fs71', 232)" class="f">downloadPrices</span> <span onmouseout="hideTip(event, 'fs72', 233)" onmouseover="showTip(event, 'fs72', 233)" class="i">from</span> <span onmouseout="hideTip(event, 'fs73', 234)" onmouseover="showTip(event, 'fs73', 234)" class="i">stock</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs74', 235)" onmouseover="showTip(event, 'fs74', 235)" class="i">async</span> {
  <span class="c">// Download price from Yahoo</span>
  <span class="k">let</span> <span onmouseout="hideTip(event, 'fs75', 236)" onmouseover="showTip(event, 'fs75', 236)" class="i">wc</span> <span class="o">=</span> <span class="k">new</span> <span onmouseout="hideTip(event, 'fs76', 237)" onmouseover="showTip(event, 'fs76', 237)" class="t">WebClient</span>()
