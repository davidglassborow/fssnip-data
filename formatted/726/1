    <span class="c">// TODO: Use date time in case it is not linear</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs51', 154)" onmouseover="showTip(event, 'fs51', 154)" class="i">xavg</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs10', 155)" onmouseover="showTip(event, 'fs10', 155)" class="f">float</span> (<span onmouseout="hideTip(event, 'fs50', 156)" onmouseover="showTip(event, 'fs50', 156)" class="i">values</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs39', 157)" onmouseover="showTip(event, 'fs39', 157)" class="i">Length</span> <span class="o">-</span> <span class="n">1</span>) <span class="o">/</span> <span class="n">2.0</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs52', 158)" onmouseover="showTip(event, 'fs52', 158)" class="i">yavg</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs13', 159)" onmouseover="showTip(event, 'fs13', 159)" class="t">Seq</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs53', 160)" onmouseover="showTip(event, 'fs53', 160)" class="f">averageBy</span> <span onmouseout="hideTip(event, 'fs23', 161)" onmouseover="showTip(event, 'fs23', 161)" class="f">snd</span> <span onmouseout="hideTip(event, 'fs50', 162)" onmouseover="showTip(event, 'fs50', 162)" class="i">values</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs54', 163)" onmouseover="showTip(event, 'fs54', 163)" class="i">sums</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs50', 164)" onmouseover="showTip(event, 'fs50', 164)" class="i">values</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs13', 165)" onmouseover="showTip(event, 'fs13', 165)" class="t">Seq</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs55', 166)" onmouseover="showTip(event, 'fs55', 166)" class="f">mapi</span> (<span class="k">fun</span> <span onmouseout="hideTip(event, 'fs56', 167)" onmouseover="showTip(event, 'fs56', 167)" class="i">x</span> (_, <span onmouseout="hideTip(event, 'fs31', 168)" onmouseover="showTip(event, 'fs31', 168)" class="i">v</span>) <span class="k">-&gt;</span> 
      (<span onmouseout="hideTip(event, 'fs10', 169)" onmouseover="showTip(event, 'fs10', 169)" class="f">float</span> <span onmouseout="hideTip(event, 'fs56', 170)" onmouseover="showTip(event, 'fs56', 170)" class="i">x</span> <span class="o">-</span> <span onmouseout="hideTip(event, 'fs51', 171)" onmouseover="showTip(event, 'fs51', 171)" class="i">xavg</span>) <span class="o">*</span> (<span onmouseout="hideTip(event, 'fs31', 172)" onmouseover="showTip(event, 'fs31', 172)" class="i">v</span> <span class="o">-</span> <span onmouseout="hideTip(event, 'fs52', 173)" onmouseover="showTip(event, 'fs52', 173)" class="i">yavg</span>), <span onmouseout="hideTip(event, 'fs57', 174)" onmouseover="showTip(event, 'fs57', 174)" class="f">pown</span> (<span onmouseout="hideTip(event, 'fs10', 175)" onmouseover="showTip(event, 'fs10', 175)" class="f">float</span> <span onmouseout="hideTip(event, 'fs56', 176)" onmouseover="showTip(event, 'fs56', 176)" class="i">x</span> <span class="o">-</span> <span onmouseout="hideTip(event, 'fs51', 177)" onmouseover="showTip(event, 'fs51', 177)" class="i">xavg</span>) <span class="n">2</span>)
