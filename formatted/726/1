  <span class="k">let</span> (<span class="o">=&amp;=</span>) <span class="o">=</span> <span onmouseout="hideTip(event, 'fs44', 140)" onmouseover="showTip(event, 'fs44', 140)" class="f">parallelAnd</span> 
  <span class="k">let</span> (<span class="o">=|=</span>) <span class="o">=</span> <span onmouseout="hideTip(event, 'fs46', 141)" onmouseover="showTip(event, 'fs46', 141)" class="f">parallelOr</span>

