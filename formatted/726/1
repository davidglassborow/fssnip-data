  <span class="k">let</span> <span onmouseout="hideTip(event, 'fs78', 240)" onmouseover="showTip(event, 'fs78', 240)" class="i">proxy</span> <span class="o">=</span> <span class="s">&quot;http://tomasp.net/tryjoinads/proxy.aspx?url=&quot;</span> <span class="o">+</span> <span onmouseout="hideTip(event, 'fs77', 241)" onmouseover="showTip(event, 'fs77', 241)" class="i">url</span>
  <span class="k">let!</span> <span onmouseout="hideTip(event, 'fs79', 242)" onmouseover="showTip(event, 'fs79', 242)" class="i">html</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs75', 243)" onmouseover="showTip(event, 'fs75', 243)" class="i">wc</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs80', 244)" onmouseover="showTip(event, 'fs80', 244)" class="f">AsyncDownloadString</span>(<span onmouseout="hideTip(event, 'fs81', 245)" onmouseover="showTip(event, 'fs81', 245)" class="t">Uri</span>(<span onmouseout="hideTip(event, 'fs78', 246)" onmouseover="showTip(event, 'fs78', 246)" class="i">proxy</span>)) 
  <span class="k">let</span> <span onmouseout="hideTip(event, 'fs82', 247)" onmouseover="showTip(event, 'fs82', 247)" class="i">lines</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs79', 248)" onmouseover="showTip(event, 'fs79', 248)" class="i">html</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs83', 249)" onmouseover="showTip(event, 'fs83', 249)" class="f">Split</span>([|<span class="s">&#39;\n&#39;</span>; <span class="s">&#39;\r&#39;</span>|], <span onmouseout="hideTip(event, 'fs84', 250)" onmouseover="showTip(event, 'fs84', 250)" class="t">StringSplitOptions</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs85', 251)" onmouseover="showTip(event, 'fs85', 251)" class="i">RemoveEmptyEntries</span>)

  <span class="c">// Return sequence that reads the prices</span>
  <span class="k">let</span> <span onmouseout="hideTip(event, 'fs86', 252)" onmouseover="showTip(event, 'fs86', 252)" class="i">data</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs87', 253)" onmouseover="showTip(event, 'fs87', 253)" class="i">seq</span> { 
    <span class="k">for</span> <span onmouseout="hideTip(event, 'fs88', 254)" onmouseover="showTip(event, 'fs88', 254)" class="i">line</span> <span class="k">in</span> <span onmouseout="hideTip(event, 'fs82', 255)" onmouseover="showTip(event, 'fs82', 255)" class="i">lines</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs13', 256)" onmouseover="showTip(event, 'fs13', 256)" class="t">Seq</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs89', 257)" onmouseover="showTip(event, 'fs89', 257)" class="f">skip</span> <span class="n">1</span> <span class="k">do</span>
      <span class="k">let</span> <span onmouseout="hideTip(event, 'fs90', 258)" onmouseover="showTip(event, 'fs90', 258)" class="i">infos</span> <span class="o">=</span> (<span onmouseout="hideTip(event, 'fs88', 259)" onmouseover="showTip(event, 'fs88', 259)" class="i">line</span><span class="o">:</span><span onmouseout="hideTip(event, 'fs91', 260)" onmouseover="showTip(event, 'fs91', 260)" class="t">string</span>)<span class="o">.</span><span class="f">Split</span>(<span class="s">&#39;,&#39;</span>)
