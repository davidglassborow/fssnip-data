  <span class="k">let</span> <span onmouseout="hideTip(event, 'fs83', 263)" onmouseover="showTip(event, 'fs83', 263)" class="i">proxy</span> <span class="o">=</span> <span class="s">&quot;http://tomasp.net/tryjoinads/proxy.aspx?url=&quot;</span> <span class="o">+</span> <span onmouseout="hideTip(event, 'fs82', 264)" onmouseover="showTip(event, 'fs82', 264)" class="i">url</span>
  <span class="k">let!</span> <span onmouseout="hideTip(event, 'fs84', 265)" onmouseover="showTip(event, 'fs84', 265)" class="i">html</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs80', 266)" onmouseover="showTip(event, 'fs80', 266)" class="i">wc</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs85', 267)" onmouseover="showTip(event, 'fs85', 267)" class="f">AsyncDownloadString</span>(<span onmouseout="hideTip(event, 'fs86', 268)" onmouseover="showTip(event, 'fs86', 268)" class="t">Uri</span>(<span onmouseout="hideTip(event, 'fs83', 269)" onmouseover="showTip(event, 'fs83', 269)" class="i">proxy</span>)) 
