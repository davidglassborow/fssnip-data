<span class="k">let</span> <span onmouseout="hideTip(event, 'fs38', 86)" onmouseover="showTip(event, 'fs38', 86)" class="i">flappy</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs8', 87)" onmouseover="showTip(event, 'fs8', 87)" class="t">FlappyBuilder</span>()

<span class="c">// Now we can implement flappy bird without mutation! </span>
<span class="c">// The &#39;flappy&#39; computation can use &#39;yield&#39; when you do not</span>
<span class="c">// want to do anything, so this is used for sleeping in the</span>
<span class="c">// starting state.</span>

<span class="k">let</span> <span class="k">rec</span> <span onmouseout="hideTip(event, 'fs39', 88)" onmouseover="showTip(event, 'fs39', 88)" class="f">starting</span> () <span class="o">=</span> <span onmouseout="hideTip(event, 'fs38', 89)" onmouseover="showTip(event, 'fs38', 89)" class="i">flappy</span> {
  <span class="k">for</span> <span onmouseout="hideTip(event, 'fs40', 90)" onmouseover="showTip(event, 'fs40', 90)" class="i">i</span> <span class="k">in</span> <span class="n">0</span> <span class="o">..</span> <span class="n">1000</span> <span class="k">do</span> <span class="k">yield</span> ()
  <span class="k">let</span> <span onmouseout="hideTip(event, 'fs41', 91)" onmouseover="showTip(event, 'fs41', 91)" class="i">flappy</span> <span class="o">=</span> { <span onmouseout="hideTip(event, 'fs42', 92)" onmouseover="showTip(event, 'fs42', 92)" class="i">X</span> <span class="o">=</span> <span class="n">30.0</span>; <span onmouseout="hideTip(event, 'fs43', 93)" onmouseover="showTip(event, 'fs43', 93)" class="i">Y</span> <span class="o">=</span> <span class="n">150.0</span>; <span class="i">VY</span> <span class="o">=</span> <span class="n">0.0</span>; <span class="i">IsAlive</span><span class="o">=</span><span class="k">true</span> }
  <span class="k">yield!</span> <span onmouseout="hideTip(event, 'fs44', 94)" onmouseover="showTip(event, 'fs44', 94)" class="f">running</span> (<span class="n">0</span>, <span onmouseout="hideTip(event, 'fs41', 95)" onmouseover="showTip(event, 'fs41', 95)" class="i">flappy</span>) }

<span class="k">and</span> <span onmouseout="hideTip(event, 'fs44', 96)" onmouseover="showTip(event, 'fs44', 96)" class="f">running</span> (<span onmouseout="hideTip(event, 'fs45', 97)" onmouseover="showTip(event, 'fs45', 97)" class="i">scroll</span>, <span onmouseout="hideTip(event, 'fs46', 98)" onmouseover="showTip(event, 'fs46', 98)" class="i">bird</span>) <span class="o">=</span> <span onmouseout="hideTip(event, 'fs38', 99)" onmouseover="showTip(event, 'fs38', 99)" class="i">flappy</span> {
