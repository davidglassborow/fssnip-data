    <span class="o">++</span> <span onmouseout="hideTip(event, 'fs15', 275)" onmouseover="showTip(event, 'fs15', 275)" class="f">sql</span> <span class="s">&quot; AND id IN &quot;</span> <span class="o">++</span> <span onmouseout="hideTip(event, 'fs38', 276)" onmouseover="showTip(event, 'fs38', 276)" class="f">``%L``</span><span class="o">&lt;</span><span onmouseout="hideTip(event, 'fs7', 277)" onmouseover="showTip(event, 'fs7', 277)" class="t">int</span>, _<span class="o">&gt;</span>
