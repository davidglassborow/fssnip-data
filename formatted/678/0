    <span onmouseout="hideTip(event, 'fs86', 229)" onmouseover="showTip(event, 'fs86', 229)" class="f">printf</span> <span class="s">&quot;[</span><span class="pf">%c</span><span class="s">] &quot;</span> <span class="o">&lt;|</span> <span onmouseout="hideTip(event, 'fs44', 230)" onmouseover="showTip(event, 'fs44', 230)" class="t">List</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs87', 231)" onmouseover="showTip(event, 'fs87', 231)" class="f">head</span> <span onmouseout="hideTip(event, 'fs83', 232)" onmouseover="showTip(event, 'fs83', 232)" class="i">ribR</span>
