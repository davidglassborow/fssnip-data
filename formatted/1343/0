<span class="c">//Ejemplo </span>
<span class="c">//Generamos una matriz</span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs42', 211)" onmouseover="showTip(event, 'fs42', 211)" class="i">a</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs2', 212)" onmouseover="showTip(event, 'fs2', 212)" class="t">matrix</span> [[ <span class="n">7.0</span>; <span class="n">1.0</span>; <span class="n">0.0</span>]
                [ <span class="n">6.0</span>; <span class="n">4.0</span>; <span class="n">2.0</span>]
                [ <span class="n">1.0</span>; <span class="n">1.0</span>; <span class="n">5.0</span>]]

<span class="c">//Obtenemos la inversa de la matriz</span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs43', 213)" onmouseover="showTip(event, 'fs43', 213)" class="i">i</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs42', 214)" onmouseover="showTip(event, 'fs42', 214)" class="i">a</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs44', 215)" onmouseover="showTip(event, 'fs44', 215)" class="f">inv</span>()

<span class="c">//Validamos que la matriz inversa sea correcta</span>
<span class="c">//  Multiplicamos la matriz inversa por la matriz original y el resultado </span>
<span class="c">//  debe ser la matriz identidad                                 </span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs45', 216)" onmouseover="showTip(event, 'fs45', 216)" class="i">x</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs42', 217)" onmouseover="showTip(event, 'fs42', 217)" class="i">a</span> <span class="o">*</span> <span onmouseout="hideTip(event, 'fs43', 218)" onmouseover="showTip(event, 'fs43', 218)" class="i">i</span>
</code></pre></td>
