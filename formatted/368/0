        <span class="i">layout</span><span class="o">.</span><span class="i">Children</span><span class="o">.</span><span class="i">Remove</span> <span class="i">gameOver</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs59', 309)" onmouseover="showTip(event, 'fs59', 309)" class="i">ignore</span>
        <span class="i">well</span><span class="o">.</span><span class="i">Clear</span>()
        <span class="k">return!</span> <span class="i">gameLoop</span> ()
        } 
    <span class="k">do</span>  <span class="i">gameLoop</span>() <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs85', 310)" onmouseover="showTip(event, 'fs85', 310)" class="i">Async</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs96', 311)" onmouseover="showTip(event, 'fs96', 311)" class="i">StartImmediate</span>

