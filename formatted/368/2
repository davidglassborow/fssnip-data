        <span class="i">layout</span><span class="o">.</span><span class="i">Children</span><span class="o">.</span><span class="i">Remove</span> <span class="i">start</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs61', 312)" onmouseover="showTip(event, 'fs61', 312)" class="i">ignore</span>
        }
        
    <span class="k">let</span> <span class="i">awaitClick</span> () <span class="o">=</span> <span class="i">this</span><span class="o">.</span><span class="i">MouseLeftButtonDown</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs88', 313)" onmouseover="showTip(event, 'fs88', 313)" class="i">Async</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs94', 314)" onmouseover="showTip(event, 'fs94', 314)" class="i">AwaitEvent</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs88', 315)" onmouseover="showTip(event, 'fs88', 315)" class="i">Async</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs95', 316)" onmouseover="showTip(event, 'fs95', 316)" class="i">Ignore</span>
    <span class="k">let</span> <span class="i">pause</span> () <span class="o">=</span>  <span onmouseout="hideTip(event, 'fs88', 317)" onmouseover="showTip(event, 'fs88', 317)" class="i">Async</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs89', 318)" onmouseover="showTip(event, 'fs89', 318)" class="i">Sleep</span> <span class="n">5000</span>

    <span class="k">let</span> <span class="k">rec</span> <span class="i">gameLoop</span> () <span class="o">=</span>  <span onmouseout="hideTip(event, 'fs86', 319)" onmouseover="showTip(event, 'fs86', 319)" class="i">async</span> {
        <span class="k">do!</span> <span class="i">prompt</span> <span class="s">&quot;Click To Start&quot;</span> <span class="i">awaitClick</span>                                    
        <span class="k">do!</span> <span class="i">inGameLoop</span> ()         
        <span class="k">do!</span> <span class="i">prompt</span> <span class="s">&quot;Game Over&quot;</span> <span class="i">pause</span>        
        <span class="i">well</span><span class="o">.</span><span class="i">Clear</span>()
        <span class="k">return!</span> <span class="i">gameLoop</span> ()
        } 
    
    <span class="k">do</span>  <span class="i">gameLoop</span>() <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs88', 320)" onmouseover="showTip(event, 'fs88', 320)" class="i">Async</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs96', 321)" onmouseover="showTip(event, 'fs96', 321)" class="i">StartImmediate</span>

