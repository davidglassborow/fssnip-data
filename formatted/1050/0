    <span class="k">match</span> <span onmouseout="hideTip(event, 'fs4', 6)" onmouseover="showTip(event, 'fs4', 6)" class="i">traitorName</span> <span class="k">with</span>
    | <span class="s">&quot;Jason&quot;</span> <span class="k">-&gt;</span> <span class="s">&quot;Jason is the traitor.&quot;</span>
    | <span class="s">&quot;Jackie&quot;</span> <span class="k">-&gt;</span> <span class="s">&quot;Jackie is the traitor.&quot;</span>
    | <span class="s">&quot;Ben&quot;</span> <span class="k">-&gt;</span> <span class="s">&quot;Ben is the traitor.&quot;</span>
    | <span class="s">&quot;Matt&quot;</span> <span class="k">-&gt;</span> <span class="s">&quot;Matt is the traitor.&quot;</span>
    | _ <span class="k">-&gt;</span> <span class="s">&quot;There are no traitors.&quot;</span>

