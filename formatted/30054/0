<span class="c">(*</span>
<span class="c">let GetMaxCorrelationCoef (matrix :coMatrix) =</span>
<span class="c">    let px = Array.init (matrix.Size - 1) (fun y -&gt; Array.sum (Array.init (matrix.Size - 1) (fun x -&gt; matrix.Matrix.[x,y])))</span>
<span class="c">    let py = Array.init (matrix.Size - 1) (fun x -&gt; Array.sum (Array.init (matrix.Size - 1) (fun y -&gt; matrix.Matrix.[x,y])))</span>
<span class="c">    let mutable Q :float [,] = Array2D.zeroCreate (matrix.Size + 1) (matrix.Size + 1)</span>
<span class="c">    for j in [1..(matrix.Size - 1)] do</span>
<span class="c">        for i in [1..(matrix.Size - 1)] do</span>
<span class="c">            Q.[i+1,j+1] &lt;- 0.0</span>
<span class="c">            for k in [1..(matrix.Size - 1)] do</span>
<span class="c">                if (not (px.[i] = 0) &amp;&amp; not (py.[j] = 0)) then</span>
<span class="c">                    Q.[i+1,j+1] &lt;- Q.[i+1,j+1] + ((float matrix.Matrix.[i,k] * float matrix.Matrix.[j,k]) / float px.[i]) / float py.[k]</span>
<span class="c">*)</span>


<span class="k">let</span> <span onmouseout="hideTip(event, 'fs95', 795)" onmouseover="showTip(event, 'fs95', 795)" class="f">GetResults</span> (<span onmouseout="hideTip(event, 'fs50', 796)" onmouseover="showTip(event, 'fs50', 796)" class="i">matrix</span> <span class="o">:</span><span onmouseout="hideTip(event, 'fs4', 797)" onmouseover="showTip(event, 'fs4', 797)" class="t">coMatrix</span>) <span class="o">=</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs96', 798)" onmouseover="showTip(event, 'fs96', 798)" class="i">ent</span> <span class="o">=</span> (<span class="s">&quot;Entropy&quot;</span>,(<span onmouseout="hideTip(event, 'fs80', 799)" onmouseover="showTip(event, 'fs80', 799)" class="f">GetEntropy</span> <span onmouseout="hideTip(event, 'fs50', 800)" onmouseover="showTip(event, 'fs50', 800)" class="i">matrix</span>))
