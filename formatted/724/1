    [| <span class="k">for</span> <span onmouseout="hideTip(event, 'fs57', 176)" onmouseover="showTip(event, 'fs57', 176)" class="i">i</span> <span class="k">in</span> <span class="n">0..</span><span class="n">10</span> <span class="k">do</span> 
