<span class="k">let</span> <span onmouseout="hideTip(event, 'fs62', 196)" onmouseover="showTip(event, 'fs62', 196)" class="i">result</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs63', 197)" onmouseover="showTip(event, 'fs63', 197)" class="t">Seq</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs67', 198)" onmouseover="showTip(event, 'fs67', 198)" class="f">filter</span> <span onmouseout="hideTip(event, 'fs65', 199)" onmouseover="showTip(event, 'fs65', 199)" class="f">isEven</span> <span onmouseout="hideTip(event, 'fs66', 200)" onmouseover="showTip(event, 'fs66', 200)" class="i">original</span>

