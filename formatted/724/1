<span class="c">//---------------------------------------------------------------</span>

<span class="c">// ---- skipping elements ---------------------------------------</span>

<span class="k">let</span> <span onmouseout="hideTip(event, 'fs61', 203)" onmouseover="showTip(event, 'fs61', 203)" class="i">original</span> <span class="o">=</span> [<span class="n">0..</span><span class="n">5</span>]
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs62', 204)" onmouseover="showTip(event, 'fs62', 204)" class="i">result</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs63', 205)" onmouseover="showTip(event, 'fs63', 205)" class="t">Seq</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs68', 206)" onmouseover="showTip(event, 'fs68', 206)" class="f">skip</span> <span class="n">2</span> <span onmouseout="hideTip(event, 'fs61', 207)" onmouseover="showTip(event, 'fs61', 207)" class="i">original</span>

