<span class="c">//---------------------------------------------------------------</span>

<span class="c">//---------------------------------------------------------------</span>
<span class="c">// Operations on Sequences</span>
<span class="c">//</span>
<span class="c">// The following sections describe the Seq module which contains</span>
<span class="c">// a bunch of functions for working with data inside of a</span>
<span class="c">// collection. Note that any operations in the Seq module work</span>
<span class="c">// on both arrays and lists.</span>
<span class="c">//---------------------------------------------------------------</span>
   
<span class="c">// ---- transforming collections with map -----------------------</span>

<span class="k">let</span> <span onmouseout="hideTip(event, 'fs60', 180)" onmouseover="showTip(event, 'fs60', 180)" class="f">square</span> <span onmouseout="hideTip(event, 'fs14', 181)" onmouseover="showTip(event, 'fs14', 181)" class="i">x</span> <span class="o">=</span>
    <span onmouseout="hideTip(event, 'fs14', 182)" onmouseover="showTip(event, 'fs14', 182)" class="i">x</span> <span class="o">*</span> <span onmouseout="hideTip(event, 'fs14', 183)" onmouseover="showTip(event, 'fs14', 183)" class="i">x</span>

<span class="k">let</span> <span onmouseout="hideTip(event, 'fs61', 184)" onmouseover="showTip(event, 'fs61', 184)" class="i">original</span> <span class="o">=</span> [<span class="n">0..</span><span class="n">5</span>]
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs62', 185)" onmouseover="showTip(event, 'fs62', 185)" class="i">result</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs63', 186)" onmouseover="showTip(event, 'fs63', 186)" class="t">Seq</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs64', 187)" onmouseover="showTip(event, 'fs64', 187)" class="f">map</span> <span onmouseout="hideTip(event, 'fs60', 188)" onmouseover="showTip(event, 'fs60', 188)" class="f">square</span> <span onmouseout="hideTip(event, 'fs61', 189)" onmouseover="showTip(event, 'fs61', 189)" class="i">original</span>

<span class="i">AssertEquality</span> <span onmouseout="hideTip(event, 'fs61', 190)" onmouseover="showTip(event, 'fs61', 190)" class="i">original</span> <span class="i">__</span>
<span class="i">AssertEquality</span> <span onmouseout="hideTip(event, 'fs62', 191)" onmouseover="showTip(event, 'fs62', 191)" class="i">result</span> <span class="i">__</span>

<span class="c">//---------------------------------------------------------------</span>
    
<span class="c">// ---- filtering collections -----------------------------------</span>
 
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs65', 192)" onmouseover="showTip(event, 'fs65', 192)" class="f">isEven</span> <span onmouseout="hideTip(event, 'fs14', 193)" onmouseover="showTip(event, 'fs14', 193)" class="i">x</span> <span class="o">=</span>
    <span onmouseout="hideTip(event, 'fs14', 194)" onmouseover="showTip(event, 'fs14', 194)" class="i">x</span> <span class="o">%</span> <span class="n">2</span> <span class="o">=</span> <span class="n">0</span>

