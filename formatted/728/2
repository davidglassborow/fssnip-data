      <span class="k">let</span> <span onmouseout="hideTip(event, 'fs111', 317)" onmouseover="showTip(event, 'fs111', 317)" class="i">infos</span> <span class="o">=</span> (<span onmouseout="hideTip(event, 'fs109', 318)" onmouseover="showTip(event, 'fs109', 318)" class="i">line</span><span class="o">:</span><span onmouseout="hideTip(event, 'fs112', 319)" onmouseover="showTip(event, 'fs112', 319)" class="t">string</span>)<span class="o">.</span><span class="f">Split</span>(<span class="s">&#39;,&#39;</span>)
