    <span class="c">// TODO: Use date time in case it is not linear</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs67', 214)" onmouseover="showTip(event, 'fs67', 214)" class="i">xavg</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs10', 215)" onmouseover="showTip(event, 'fs10', 215)" class="f">float</span> (<span onmouseout="hideTip(event, 'fs19', 216)" onmouseover="showTip(event, 'fs19', 216)" class="i">values</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs29', 217)" onmouseover="showTip(event, 'fs29', 217)" class="i">Length</span> <span class="o">-</span> <span class="n">1</span>) <span class="o">/</span> <span class="n">2.0</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs68', 218)" onmouseover="showTip(event, 'fs68', 218)" class="i">yavg</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs34', 219)" onmouseover="showTip(event, 'fs34', 219)" class="t">Seq</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs69', 220)" onmouseover="showTip(event, 'fs69', 220)" class="f">averageBy</span> <span onmouseout="hideTip(event, 'fs36', 221)" onmouseover="showTip(event, 'fs36', 221)" class="f">snd</span> <span onmouseout="hideTip(event, 'fs19', 222)" onmouseover="showTip(event, 'fs19', 222)" class="i">values</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs70', 223)" onmouseover="showTip(event, 'fs70', 223)" class="i">sums</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs19', 224)" onmouseover="showTip(event, 'fs19', 224)" class="i">values</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs34', 225)" onmouseover="showTip(event, 'fs34', 225)" class="t">Seq</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs71', 226)" onmouseover="showTip(event, 'fs71', 226)" class="f">mapi</span> (<span class="k">fun</span> <span onmouseout="hideTip(event, 'fs72', 227)" onmouseover="showTip(event, 'fs72', 227)" class="i">x</span> (_, <span onmouseout="hideTip(event, 'fs46', 228)" onmouseover="showTip(event, 'fs46', 228)" class="i">v</span>) <span class="k">-&gt;</span> 
      (<span onmouseout="hideTip(event, 'fs10', 229)" onmouseover="showTip(event, 'fs10', 229)" class="f">float</span> <span onmouseout="hideTip(event, 'fs72', 230)" onmouseover="showTip(event, 'fs72', 230)" class="i">x</span> <span class="o">-</span> <span onmouseout="hideTip(event, 'fs67', 231)" onmouseover="showTip(event, 'fs67', 231)" class="i">xavg</span>) <span class="o">*</span> (<span onmouseout="hideTip(event, 'fs46', 232)" onmouseover="showTip(event, 'fs46', 232)" class="i">v</span> <span class="o">-</span> <span onmouseout="hideTip(event, 'fs68', 233)" onmouseover="showTip(event, 'fs68', 233)" class="i">yavg</span>), <span onmouseout="hideTip(event, 'fs73', 234)" onmouseover="showTip(event, 'fs73', 234)" class="f">pown</span> (<span onmouseout="hideTip(event, 'fs10', 235)" onmouseover="showTip(event, 'fs10', 235)" class="f">float</span> <span onmouseout="hideTip(event, 'fs72', 236)" onmouseover="showTip(event, 'fs72', 236)" class="i">x</span> <span class="o">-</span> <span onmouseout="hideTip(event, 'fs67', 237)" onmouseover="showTip(event, 'fs67', 237)" class="i">xavg</span>) <span class="n">2</span>)
