  <span class="c">// Return sequence that reads the prices</span>
  <span class="k">let</span> <span onmouseout="hideTip(event, 'fs107', 311)" onmouseover="showTip(event, 'fs107', 311)" class="i">data</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs108', 312)" onmouseover="showTip(event, 'fs108', 312)" class="i">seq</span> { 
