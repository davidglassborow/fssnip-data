  <span class="k">let!</span> <span onmouseout="hideTip(event, 'fs99', 299)" onmouseover="showTip(event, 'fs99', 299)" class="i">html</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs95', 300)" onmouseover="showTip(event, 'fs95', 300)" class="i">wc</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs100', 301)" onmouseover="showTip(event, 'fs100', 301)" class="f">AsyncDownloadString</span>(<span onmouseout="hideTip(event, 'fs101', 302)" onmouseover="showTip(event, 'fs101', 302)" class="t">Uri</span>(<span onmouseout="hideTip(event, 'fs98', 303)" onmouseover="showTip(event, 'fs98', 303)" class="i">proxy</span>)) 
