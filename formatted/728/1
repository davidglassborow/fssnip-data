      (<span onmouseout="hideTip(event, 'fs10', 227)" onmouseover="showTip(event, 'fs10', 227)" class="f">float</span> <span onmouseout="hideTip(event, 'fs71', 228)" onmouseover="showTip(event, 'fs71', 228)" class="i">x</span> <span class="o">-</span> <span onmouseout="hideTip(event, 'fs66', 229)" onmouseover="showTip(event, 'fs66', 229)" class="i">xavg</span>) <span class="o">*</span> (<span onmouseout="hideTip(event, 'fs45', 230)" onmouseover="showTip(event, 'fs45', 230)" class="i">v</span> <span class="o">-</span> <span onmouseout="hideTip(event, 'fs67', 231)" onmouseover="showTip(event, 'fs67', 231)" class="i">yavg</span>), <span onmouseout="hideTip(event, 'fs72', 232)" onmouseover="showTip(event, 'fs72', 232)" class="f">pown</span> (<span onmouseout="hideTip(event, 'fs10', 233)" onmouseover="showTip(event, 'fs10', 233)" class="f">float</span> <span onmouseout="hideTip(event, 'fs71', 234)" onmouseover="showTip(event, 'fs71', 234)" class="i">x</span> <span class="o">-</span> <span onmouseout="hideTip(event, 'fs66', 235)" onmouseover="showTip(event, 'fs66', 235)" class="i">xavg</span>) <span class="n">2</span>)
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs73', 236)" onmouseover="showTip(event, 'fs73', 236)" class="i">v1</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs33', 237)" onmouseover="showTip(event, 'fs33', 237)" class="t">Seq</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs74', 238)" onmouseover="showTip(event, 'fs74', 238)" class="f">sumBy</span> <span onmouseout="hideTip(event, 'fs75', 239)" onmouseover="showTip(event, 'fs75', 239)" class="f">fst</span> <span onmouseout="hideTip(event, 'fs69', 240)" onmouseover="showTip(event, 'fs69', 240)" class="i">sums</span>
