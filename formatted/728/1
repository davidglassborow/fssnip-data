    <span class="c">// TODO: Use date time in case it is not linear</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs66', 212)" onmouseover="showTip(event, 'fs66', 212)" class="i">xavg</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs10', 213)" onmouseover="showTip(event, 'fs10', 213)" class="f">float</span> (<span onmouseout="hideTip(event, 'fs19', 214)" onmouseover="showTip(event, 'fs19', 214)" class="i">values</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs28', 215)" onmouseover="showTip(event, 'fs28', 215)" class="i">Length</span> <span class="o">-</span> <span class="n">1</span>) <span class="o">/</span> <span class="n">2.0</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs67', 216)" onmouseover="showTip(event, 'fs67', 216)" class="i">yavg</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs33', 217)" onmouseover="showTip(event, 'fs33', 217)" class="t">Seq</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs68', 218)" onmouseover="showTip(event, 'fs68', 218)" class="f">averageBy</span> <span onmouseout="hideTip(event, 'fs35', 219)" onmouseover="showTip(event, 'fs35', 219)" class="f">snd</span> <span onmouseout="hideTip(event, 'fs19', 220)" onmouseover="showTip(event, 'fs19', 220)" class="i">values</span>
