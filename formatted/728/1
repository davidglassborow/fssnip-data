  <span class="c">/// Checks that one of the properties holds for subsequent parts of the input</span>
  <span class="k">let</span> <span onmouseout="hideTip(event, 'fs62', 177)" onmouseover="showTip(event, 'fs62', 177)" class="f">sequenceOr</span> <span onmouseout="hideTip(event, 'fs57', 178)" onmouseover="showTip(event, 'fs57', 178)" class="i">a</span> <span onmouseout="hideTip(event, 'fs58', 179)" onmouseover="showTip(event, 'fs58', 179)" class="i">b</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs23', 180)" onmouseover="showTip(event, 'fs23', 180)" class="f">sequence</span> <span onmouseout="hideTip(event, 'fs57', 181)" onmouseover="showTip(event, 'fs57', 181)" class="i">a</span> <span onmouseout="hideTip(event, 'fs58', 182)" onmouseover="showTip(event, 'fs58', 182)" class="i">b</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs11', 183)" onmouseover="showTip(event, 'fs11', 183)" class="f">map</span> (<span class="k">fun</span> (<span onmouseout="hideTip(event, 'fs59', 184)" onmouseover="showTip(event, 'fs59', 184)" class="i">a</span>, <span onmouseout="hideTip(event, 'fs60', 185)" onmouseover="showTip(event, 'fs60', 185)" class="i">b</span>) <span class="k">-&gt;</span> <span onmouseout="hideTip(event, 'fs59', 186)" onmouseover="showTip(event, 'fs59', 186)" class="i">a</span> <span class="o">||</span> <span onmouseout="hideTip(event, 'fs60', 187)" onmouseover="showTip(event, 'fs60', 187)" class="i">b</span>)
