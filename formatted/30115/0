<span class="c">(*</span>
<span class="c">let ``example usage`` =</span>
<span class="c">    transactionWith &lt;| fun () -&gt; </span>
<span class="c">        Console.WriteLine &quot;Normal source code in transaction&quot;</span>
<span class="c">        Console.WriteLine &quot;e.g. database operations!&quot;</span>
<span class="c">        Console.WriteLine &quot;Return values and &quot;</span>
<span class="c">        Console.WriteLine &quot;C# Task classes supported.&quot;</span>

<span class="c">let ``example usage FSharpAsync`` =</span>
<span class="c">    transactionWithAsync &lt;| fun () -&gt; async {</span>
<span class="c">        Console.WriteLine &quot;Async source code in transaction&quot;</span>
<span class="c">        return &quot;hello!&quot;</span>
<span class="c">    }</span>
<span class="c">exampleusageFSharpAsync |&gt; Async.RunSynchronously;;</span>
<span class="c">*)</span>
</code></pre></td>
</tr>
