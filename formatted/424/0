    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs54', 219)" onmouseover="showTip(event, 'fs54', 219)" class="i">absmin</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs15', 220)" onmouseover="showTip(event, 'fs15', 220)" class="f">abs</span> <span onmouseout="hideTip(event, 'fs52', 221)" onmouseover="showTip(event, 'fs52', 221)" class="i">zmin</span>
