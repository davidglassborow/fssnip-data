                <span class="k">if</span> ((<span onmouseout="hideTip(event, 'fs71', 625)" onmouseover="showTip(event, 'fs71', 625)" class="i">i</span> <span class="o">%</span> <span onmouseout="hideTip(event, 'fs132', 626)" onmouseover="showTip(event, 'fs132', 626)" class="i">Nk</span>) <span class="o">=</span> <span class="n">0</span>) <span class="k">then</span>
                    <span onmouseout="hideTip(event, 'fs109', 627)" onmouseover="showTip(event, 'fs109', 627)" class="f">printfn</span> <span class="s">&quot;&quot;</span>
                    <span onmouseout="hideTip(event, 'fs108', 628)" onmouseover="showTip(event, 'fs108', 628)" class="f">printf</span> <span class="s">&quot;</span><span class="pf">%f</span><span class="s"></span><span class="e">\t</span><span class="s">&quot;</span> <span onmouseout="hideTip(event, 'fs122', 629)" onmouseover="showTip(event, 'fs122', 629)" class="i">pInterval</span><span class="o">.</span>[<span onmouseout="hideTip(event, 'fs71', 630)" onmouseover="showTip(event, 'fs71', 630)" class="i">i</span><span class="o">/</span><span onmouseout="hideTip(event, 'fs132', 631)" onmouseover="showTip(event, 'fs132', 631)" class="i">Nk</span>]

                <span onmouseout="hideTip(event, 'fs108', 632)" onmouseover="showTip(event, 'fs108', 632)" class="f">printf</span> <span class="s">&quot;</span><span class="pf">%f</span><span class="s"></span><span class="e">\t</span><span class="s">&quot;</span> (<span onmouseout="hideTip(event, 'fs129', 633)" onmouseover="showTip(event, 'fs129', 633)" class="i">el</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs92', 634)" onmouseover="showTip(event, 'fs92', 634)" class="t">Seq</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs131', 635)" onmouseover="showTip(event, 'fs131', 635)" class="f">averageBy</span> (<span onmouseout="hideTip(event, 'fs128', 636)" onmouseover="showTip(event, 'fs128', 636)" class="f">snd</span> <span class="o">&gt;</span><span class="o">&gt;</span> <span onmouseout="hideTip(event, 'fs128', 637)" onmouseover="showTip(event, 'fs128', 637)" class="f">snd</span> <span class="o">&gt;</span><span class="o">&gt;</span> <span onmouseout="hideTip(event, 'fs123', 638)" onmouseover="showTip(event, 'fs123', 638)" class="f">float</span>))
            )
    



[&lt;<span onmouseout="hideTip(event, 'fs136', 639)" onmouseover="showTip(event, 'fs136', 639)" class="t">EntryPoint</span>&gt;]
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs137', 640)" onmouseover="showTip(event, 'fs137', 640)" class="f">main</span> <span onmouseout="hideTip(event, 'fs138', 641)" onmouseover="showTip(event, 'fs138', 641)" class="i">argv</span> <span class="o">=</span> 

  
    <span class="c">//*****************************************</span>
    <span class="c">//*            MODEL PARAMETERS           *</span>
    <span class="c">//*****************************************</span>
    
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs139', 642)" onmouseover="showTip(event, 'fs139', 642)" class="i">airplaneModel</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs98', 643)" onmouseover="showTip(event, 'fs98', 643)" class="f">oneCorridorAirplaneModel</span> <span class="n">40</span> <span class="n">3</span>;      <span class="c">// Airplane model: 40 rows with 3 seats on each side of the central corridor</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs122', 644)" onmouseover="showTip(event, 'fs122', 644)" class="i">pInterval</span> <span class="o">=</span> [| <span class="n">0.0</span> <span class="o">..</span> <span class="n">0.05</span> <span class="o">..</span> <span class="n">1.0</span> |]                <span class="c">// p -- probability of giving way to a person behind you in corridor</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs140', 645)" onmouseover="showTip(event, 'fs140', 645)" class="i">NInterval</span> <span class="o">=</span> [| <span class="n">0</span> <span class="o">..</span> <span class="n">5</span> <span class="o">..</span> <span class="n">100</span> |]                     <span class="c">// N -- after stepping into the corridor a person blocks way for 0 to N turns</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs141', 646)" onmouseover="showTip(event, 'fs141', 646)" class="i">kIterations</span> <span class="o">=</span> <span class="n">30</span>                                    <span class="c">// number of iterations of simulations with the same p and N value to calculate averages</span>

    <span class="c">// ****************************************</span>

    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs142', 647)" onmouseover="showTip(event, 'fs142', 647)" class="i">stopwatch</span> <span class="o">=</span> <span class="k">new</span> <span onmouseout="hideTip(event, 'fs34', 648)" onmouseover="showTip(event, 'fs34', 648)" class="i">System</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs143', 649)" onmouseover="showTip(event, 'fs143', 649)" class="i">Diagnostics</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs144', 650)" onmouseover="showTip(event, 'fs144', 650)" class="t">Stopwatch</span>();
    <span onmouseout="hideTip(event, 'fs142', 651)" onmouseover="showTip(event, 'fs142', 651)" class="i">stopwatch</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs145', 652)" onmouseover="showTip(event, 'fs145', 652)" class="f">Start</span>();

    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs146', 653)" onmouseover="showTip(event, 'fs146', 653)" class="i">results</span> <span class="o">=</span> 
        <span onmouseout="hideTip(event, 'fs82', 654)" onmouseover="showTip(event, 'fs82', 654)" class="i">seq</span> { 
            <span class="k">for</span> <span onmouseout="hideTip(event, 'fs44', 655)" onmouseover="showTip(event, 'fs44', 655)" class="i">p</span> <span class="k">in</span> <span onmouseout="hideTip(event, 'fs122', 656)" onmouseover="showTip(event, 'fs122', 656)" class="i">pInterval</span> <span class="k">do</span>
                <span class="k">for</span> <span onmouseout="hideTip(event, 'fs38', 657)" onmouseover="showTip(event, 'fs38', 657)" class="i">N</span> <span class="k">in</span> <span onmouseout="hideTip(event, 'fs140', 658)" onmouseover="showTip(event, 'fs140', 658)" class="i">NInterval</span> <span class="k">do</span>
                    <span class="k">yield</span> <span onmouseout="hideTip(event, 'fs147', 659)" onmouseover="showTip(event, 'fs147', 659)" class="i">async</span> { 
                        <span class="k">let</span> <span onmouseout="hideTip(event, 'fs33', 660)" onmouseover="showTip(event, 'fs33', 660)" class="i">random</span> <span class="o">=</span> <span class="k">new</span> <span onmouseout="hideTip(event, 'fs34', 661)" onmouseover="showTip(event, 'fs34', 661)" class="i">System</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs35', 662)" onmouseover="showTip(event, 'fs35', 662)" class="t">Random</span>()
                        <span class="k">let</span> <span onmouseout="hideTip(event, 'fs111', 663)" onmouseover="showTip(event, 'fs111', 663)" class="f">transformation</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs45', 664)" onmouseover="showTip(event, 'fs45', 664)" class="f">transform</span> <span onmouseout="hideTip(event, 'fs33', 665)" onmouseover="showTip(event, 'fs33', 665)" class="i">random</span> { <span onmouseout="hideTip(event, 'fs24', 666)" onmouseover="showTip(event, 'fs24', 666)" class="i">p</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs44', 667)" onmouseover="showTip(event, 'fs44', 667)" class="i">p</span>; <span onmouseout="hideTip(event, 'fs26', 668)" onmouseover="showTip(event, 'fs26', 668)" class="i">N</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs38', 669)" onmouseover="showTip(event, 'fs38', 669)" class="i">N</span> }
                        <span onmouseout="hideTip(event, 'fs109', 670)" onmouseover="showTip(event, 'fs109', 670)" class="f">printfn</span> <span class="s">&quot;Calculations for [ p=</span><span class="pf">%f</span><span class="s">   N=</span><span class="pf">%d</span><span class="s"> ] started&quot;</span> <span onmouseout="hideTip(event, 'fs44', 671)" onmouseover="showTip(event, 'fs44', 671)" class="i">p</span> <span onmouseout="hideTip(event, 'fs38', 672)" onmouseover="showTip(event, 'fs38', 672)" class="i">N</span>
                        <span class="k">return</span> <span onmouseout="hideTip(event, 'fs82', 673)" onmouseover="showTip(event, 'fs82', 673)" class="i">seq</span> { 
                            <span class="k">for</span> <span onmouseout="hideTip(event, 'fs71', 674)" onmouseover="showTip(event, 'fs71', 674)" class="i">i</span> <span class="k">in</span> <span class="n">1</span> <span class="o">..</span> <span onmouseout="hideTip(event, 'fs141', 675)" onmouseover="showTip(event, 'fs141', 675)" class="i">kIterations</span> <span class="k">do</span> <span class="k">yield</span> ((<span onmouseout="hideTip(event, 'fs44', 676)" onmouseover="showTip(event, 'fs44', 676)" class="i">p</span>, <span onmouseout="hideTip(event, 'fs38', 677)" onmouseover="showTip(event, 'fs38', 677)" class="i">N</span>), <span onmouseout="hideTip(event, 'fs110', 678)" onmouseover="showTip(event, 'fs110', 678)" class="f">runSimulationOnce</span> <span onmouseout="hideTip(event, 'fs111', 679)" onmouseover="showTip(event, 'fs111', 679)" class="f">transformation</span> <span onmouseout="hideTip(event, 'fs139', 680)" onmouseover="showTip(event, 'fs139', 680)" class="i">airplaneModel</span>) 
                            } <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs92', 681)" onmouseover="showTip(event, 'fs92', 681)" class="t">Seq</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs148', 682)" onmouseover="showTip(event, 'fs148', 682)" class="f">toList</span> 
                    } 
            }
            <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs149', 683)" onmouseover="showTip(event, 'fs149', 683)" class="t">Async</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs150', 684)" onmouseover="showTip(event, 'fs150', 684)" class="f">Parallel</span> 
            <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs149', 685)" onmouseover="showTip(event, 'fs149', 685)" class="t">Async</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs151', 686)" onmouseover="showTip(event, 'fs151', 686)" class="f">RunSynchronously</span>

    <span onmouseout="hideTip(event, 'fs120', 687)" onmouseover="showTip(event, 'fs120', 687)" class="f">printResults</span> <span onmouseout="hideTip(event, 'fs146', 688)" onmouseover="showTip(event, 'fs146', 688)" class="i">results</span> <span onmouseout="hideTip(event, 'fs122', 689)" onmouseover="showTip(event, 'fs122', 689)" class="i">pInterval</span> <span onmouseout="hideTip(event, 'fs140', 690)" onmouseover="showTip(event, 'fs140', 690)" class="i">NInterval</span>

    <span onmouseout="hideTip(event, 'fs142', 691)" onmouseover="showTip(event, 'fs142', 691)" class="i">stopwatch</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs152', 692)" onmouseover="showTip(event, 'fs152', 692)" class="f">Stop</span>();
    
