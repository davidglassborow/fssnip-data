            | { <span class="i">kind</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs16', 230)" onmouseover="showTip(event, 'fs16', 230)" class="p">Corridor</span>; <span class="i">person</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs62', 231)" onmouseover="showTip(event, 'fs62', 231)" class="p">None</span> } <span class="k">-&gt;</span> <span onmouseout="hideTip(event, 'fs54', 232)" onmouseover="showTip(event, 'fs54', 232)" class="f">getNewOccupant</span> <span onmouseout="hideTip(event, 'fs46', 233)" onmouseover="showTip(event, 'fs46', 233)" class="i">behavior</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs24', 234)" onmouseover="showTip(event, 'fs24', 234)" class="i">p</span> <span onmouseout="hideTip(event, 'fs46', 235)" onmouseover="showTip(event, 'fs46', 235)" class="i">behavior</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs26', 236)" onmouseover="showTip(event, 'fs26', 236)" class="i">N</span> <span onmouseout="hideTip(event, 'fs47', 237)" onmouseover="showTip(event, 'fs47', 237)" class="i">model</span><span class="o">.</span>[<span onmouseout="hideTip(event, 'fs71', 238)" onmouseover="showTip(event, 'fs71', 238)" class="i">i</span><span class="o">-</span><span class="n">1</span>, <span onmouseout="hideTip(event, 'fs72', 239)" onmouseover="showTip(event, 'fs72', 239)" class="i">j</span>]<span class="o">.</span><span class="i">person</span> <span onmouseout="hideTip(event, 'fs47', 240)" onmouseover="showTip(event, 'fs47', 240)" class="i">model</span><span class="o">.</span>[<span onmouseout="hideTip(event, 'fs71', 241)" onmouseover="showTip(event, 'fs71', 241)" class="i">i</span>, <span onmouseout="hideTip(event, 'fs72', 242)" onmouseover="showTip(event, 'fs72', 242)" class="i">j</span><span class="o">-</span><span class="n">1</span>]<span class="o">.</span><span class="i">person</span> <span onmouseout="hideTip(event, 'fs47', 243)" onmouseover="showTip(event, 'fs47', 243)" class=