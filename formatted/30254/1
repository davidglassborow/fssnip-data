    <span onmouseout="hideTip(event, 'fs137', 329)" onmouseover="showTip(event, 'fs137', 329)" class="t">Console</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs138', 330)" onmouseover="showTip(event, 'fs138', 330)" class="f">WriteLine</span> <span class="s">&quot;Store users private key to a cold dry place and never show it to anyone:&quot;</span>
    <span onmouseout="hideTip(event, 'fs4', 331)" onmouseover="showTip(event, 'fs4', 331)" class="i">network</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs136', 332)" onmouseover="showTip(event, 'fs136', 332)" class="i">user</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs139', 333)" onmouseover="showTip(event, 'fs139', 333)" class="i">PrivateKey</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs140', 334)" onmouseover="showTip(event, 'fs140', 334)" class="f">GetWif</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs137', 335)" onmouseover="showTip(event, 'fs137', 335)" class="t">Console</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs138', 336)" onmouseover="showTip(event, 'fs138', 336)" class="f">WriteLine</span>
