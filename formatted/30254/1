     <span onmouseout="hideTip(event, 'fs109', 342)" onmouseover="showTip(event, 'fs109', 342)" class="i">tx</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs144', 343)" onmouseover="showTip(event, 'fs144', 343)" class="i">Outputs</span> 
     <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs51', 344)" onmouseover="showTip(event, 'fs51', 344)" class="t">Seq</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs145', 345)" onmouseover="showTip(event, 'fs145', 345)" class="f">mapi</span>(<span class="k">fun</span> <span onmouseout="hideTip(event, 'fs146', 346)" onmouseover="showTip(event, 'fs146', 346)" class="i">idx</span> <span onmouseout="hideTip(event, 'fs147', 347)" onmouseover="showTip(event, 'fs147', 347)" class="i">op</span> <span class="k">-&gt;</span> <span onmouseout="hideTip(event, 'fs146', 348)" onmouseover="showTip(event, 'fs146', 348)" class="i">idx</span>,<span onmouseout="hideTip(event, 'fs147', 349)" onmouseover="showTip(event, 'fs147', 349)" class="i">op</span>)
