<span class="c">//comulative distribution for Sum(1,inf, d(i)/i^pow), </span>
<span class="c">//where P(d(i)=0) = P(d(i)=1) = 0.5 </span>
<span class="c">//tol - converge criteria 10^(-tol) </span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs94', 370)" onmouseover="showTip(event, 'fs94', 370)" class="f">distrib</span> <span onmouseout="hideTip(event, 'fs38', 371)" onmouseover="showTip(event, 'fs38', 371)" class="i">pow</span> <span onmouseout="hideTip(event, 'fs95', 372)" onmouseover="showTip(event, 'fs95', 372)" class="i">tol</span> <span onmouseout="hideTip(event, 'fs11', 373)" onmouseover="showTip(event, 'fs11', 373)" class="i">x</span> <span class="o">=</span> 
   <span class="k">let</span> <span class="k">rec</span> <span onmouseout="hideTip(event, 'fs96', 374)" onmouseover="showTip(event, 'fs96', 374)" class="f">loop</span> <span onmouseout="hideTip(event, 'fs97', 375)" onmouseover="showTip(event, 'fs97', 375)" class="i">cur</span> <span onmouseout="hideTip(event, 'fs98', 376)" onmouseover="showTip(event, 'fs98', 376)" class="i">prev</span> <span onmouseout="hideTip(event, 'fs17', 377)" onmouseover="showTip(event, 'fs17', 377)" class="i">n</span> <span class="o">=</span>
      <span class="k">if</span> <span onmouseout="hideTip(event, 'fs14', 378)" onmouseover="showTip(event, 'fs14', 378)" class="f">abs</span> (<span onmouseout="hideTip(event, 'fs98', 379)" onmouseover="showTip(event, 'fs98', 379)" class="i">prev</span><span class="o">-</span><span onmouseout="hideTip(event, 'fs97', 380)" onmouseover="showTip(event, 'fs97', 380)" class="i">cur</span>)<span class="o">&gt;</span><span onmouseout="hideTip(event, 'fs57', 381)" onmouseover="showTip(event, 'fs57', 381)" class="f">pown</span> <span class="n">0.1</span> <span onmouseout="hideTip(event, 'fs95', 382)" onmouseover="showTip(event, 'fs95', 382)" class="i">tol</span> <span class="k">then</span> <span onmouseout="hideTip(event, 'fs96', 383)" onmouseover="showTip(event, 'fs96', 383)" class="f">loop</span> (<span onmouseout="hideTip(event, 'fs65', 384)" onmouseover="showTip(event, 'fs65', 384)" class="f">distribApprox</span> <span onmouseout="hideTip(event, 'fs38', 385)" onmouseover="showTip(event, 'fs38', 385)" class="i">pow</span> <span onmouseout="hideTip(event, 'fs17', 386)" onmouseover="showTip(event, 'fs17', 386)" class="i">n</span> <span onmouseout="hideTip(event, 'fs11', 387)" onmouseover="showTip(event, 'fs11', 387)" class="i">x</span>) <span onmouseout="hideTip(event, 'fs97', 388)" onmouseover="showTip(event, 'fs97', 388)" class="i">cur</span> (<span onmouseout="hideTip(event, 'fs17', 389)" onmouseover="showTip(event, 'fs17', 389)" class="i">n</span><span class="o">+</span><span class="n">1</span>)
      <span class="k">else</span> <span onmouseout="hideTip(event, 'fs97', 390)" onmouseover="showTip(event, 'fs97', 390)" class="i">cur</span>
