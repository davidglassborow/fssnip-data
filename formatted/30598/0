<span class="c">//comulative distribution for Sum(1,inf, d(i)/i^pow), </span>
<span class="c">//where P(d(i)=0) = P(d(i)=1) = 0.5 </span>
<span class="c">//tol - converge criteria 10^(-tol) </span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs90', 362)" onmouseover="showTip(event, 'fs90', 362)" class="f">distrib</span> <span onmouseout="hideTip(event, 'fs33', 363)" onmouseover="showTip(event, 'fs33', 363)" class="i">pow</span> <span onmouseout="hideTip(event, 'fs91', 364)" onmouseover="showTip(event, 'fs91', 364)" class="i">tol</span> <span onmouseout="hideTip(event, 'fs6', 365)" onmouseover="showTip(event, 'fs6', 365)" class="i">x</span> <span class="o">=</span> 
   <span class="k">let</span> <span class="k">rec</span> <span onmouseout="hideTip(event, 'fs92', 366)" onmouseover="showTip(event, 'fs92', 366)" class="f">loop</span> <span onmouseout="hideTip(event, 'fs93', 367)" onmouseover="showTip(event, 'fs93', 367)" class="i">cur</span> <span onmouseout="hideTip(event, 'fs94', 368)" onmouseover="showTip(event, 'fs94', 368)" class="i">prev</span> <span onmouseout="hideTip(event, 'fs12', 369)" onmouseover="showTip(event, 'fs12', 369)" class="i">n</span> <span class="o">=</span>
      <span class="k">if</span> <span onmouseout="hideTip(event, 'fs9', 370)" onmouseover="showTip(event, 'fs9', 370)" class="f">abs</span> (<span onmouseout="hideTip(event, 'fs94', 371)" onmouseover="showTip(event, 'fs94', 371)" class="i">prev</span><span class="o">-</span><span onmouseout="hideTip(event, 'fs93', 372)" onmouseover="showTip(event, 'fs93', 372)" class="i">cur</span>)<span class="o">&gt;</span><span onmouseout="hideTip(event, 'fs53', 373)" onmouseover="showTip(event, 'fs53', 373)" class="f">pown</span> <span class="n">0.1</span> <span onmouseout="hideTip(event, 'fs91', 374)" onmouseover="showTip(event, 'fs91', 374)" class="i">tol</span> <span class="k">then</span> <span onmouseout="hideTip(event, 'fs92', 375)" onmouseover="showTip(event, 'fs92', 375)" class="f">loop</span> (<span onmouseout="hideTip(event, 'fs61', 376)" onmouseover="showTip(event, 'fs61', 376)" class="f">distribApprox</span> <span onmouseout="hideTip(event, 'fs33', 377)" onmouseover="showTip(event, 'fs33', 377)" class="i">pow</span> <span onmouseout="hideTip(event, 'fs12', 378)" onmouseover="showTip(event, 'fs12', 378)" class="i">n</span> <span onmouseout="hideTip(event, 'fs6', 379)" onmouseover="showTip(event, 'fs6', 379)" class="i">x</span>) <span onmouseout="hideTip(event, 'fs93', 380)" onmouseover="showTip(event, 'fs93', 380)" class="i">cur</span> (<span onmouseout="hideTip(event, 'fs12', 381)" onmouseover="showTip(event, 'fs12', 381)" class="i">n</span><span class="o">+</span><span class="n">1</span>)
      <span class="k">else</span> <span onmouseout="hideTip(event, 'fs93', 382)" onmouseover="showTip(event, 'fs93', 382)" class="i">cur</span>
   <span class="n">1.</span> <span class="o">-</span> <span onmouseout="hideTip(event, 'fs92', 383)" onmouseover="showTip(event, 'fs92', 383)" class="f">loop</span> <span class="n">0.0</span> <span class="n">1.0</span> <span class="n">1</span>         

<span class="c">//write to a file   </span>
