<span class="k">let</span> <span onmouseout="hideTip(event, 'fs92', 368)" onmouseover="showTip(event, 'fs92', 368)" class="i">root</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs45', 369)" onmouseover="showTip(event, 'fs45', 369)" class="f">makeTree</span> <span onmouseout="hideTip(event, 'fs11', 370)" onmouseover="showTip(event, 'fs11', 370)" class="i">initialState</span>
