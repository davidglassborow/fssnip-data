    <span class="c">/// Read the current state (int) and return it as &#39;int&#39;</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs116', 282)" onmouseover="showTip(event, 'fs116', 282)" class="i">read</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs30', 283)" onmouseover="showTip(event, 'fs30', 283)" class="p">DelayedUM</span> ( <span class="k">fun</span>() <span class="k">-&gt;</span> <span onmouseout="hideTip(event, 'fs29', 284)" onmouseover="showTip(event, 'fs29', 284)" class="p">UM</span> (<span class="k">fun</span> (<span onmouseout="hideTip(event, 'fs117', 285)" onmouseover="showTip(event, 'fs117', 285)" class="i">s</span><span class="o">:</span><span class="o">&#39;</span><span class="i">T</span>) <span class="k">-&gt;</span> <span onmouseout="hideTip(event, 'fs25', 286)" onmouseover="showTip(event, 'fs25', 286)" class="p">Success</span> (<span onmouseout="hideTip(event, 'fs111', 287)" onmouseover="showTip(event, 'fs111', 287)" class="p">NoUpdate</span>, <span onmouseout="hideTip(event, 'fs117', 288)" onmouseover="showTip(event, 'fs117', 288)" class="i">s</span>)) )
    <span class="c">/// Run computation and return the result </span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs118', 289)" onmouseover="showTip(event, 'fs118', 289)" class="f">readRun</span> (<span onmouseout="hideTip(event, 'fs117', 290)" onmouseover="showTip(event, 'fs117', 290)" class="i">s</span><span class="o">:</span><span class="o">&#39;</span><span class="i">T</span>) (<span onmouseout="hideTip(event, 'fs30', 291)" onmouseover="showTip(event, 'fs30', 291)" class="p">DelayedUM</span> <span onmouseout="hideTip(event, 'fs119', 292)" onmouseover="showTip(event, 'fs119', 292)" class="f">f</span>) <span class="o">=</span> 
