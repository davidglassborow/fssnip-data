    <span class="k">module</span> <span onmouseout="hideTip(event, 'fs37', 92)" onmouseover="showTip(event, 'fs37', 92)" class="t">Demo</span> <span class="o">=</span>

        <span class="k">open</span> <span onmouseout="hideTip(event, 'fs2', 93)" onmouseover="showTip(event, 'fs2', 93)" class="i">System</span>
        <span class="k">open</span> <span onmouseout="hideTip(event, 'fs2', 94)" onmouseover="showTip(event, 'fs2', 94)" class="i">System</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs38', 95)" onmouseover="showTip(event, 'fs38', 95)" class="i">Drawing</span>
        <span class="k">open</span> <span onmouseout="hideTip(event, 'fs39', 96)" onmouseover="showTip(event, 'fs39', 96)" class="i">LatinSquares</span>

        <span class="c">// [[1; 2; 3]</span>
        <span class="c">//  [2; 3; 1]</span>
        <span class="c">//  [3; 1; 2]]</span>
        <span class="k">let</span> <span onmouseout="hideTip(event, 'fs40', 97)" onmouseover="showTip(event, 'fs40', 97)" class="i">ints3x3</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs3', 98)" onmouseover="showTip(event, 'fs3', 98)" class="f">LatinSquare</span>([|<span class="n">1..</span><span class="n">3</span>|])

        <span class="c">// [[1; 2; 3; 4; 5; 6; 7]</span>
        <span class="c">//  [5; 4; 2; 1; 6; 7; 3]</span>
        <span class="c">//  [4; 6; 5; 7; 1; 3; 2]</span>
        <span class="c">//  [7; 1; 4; 3; 2; 5; 6]</span>
        <span class="c">//  [3; 7; 1; 6; 4; 2; 5]</span>
