<td class="snippet"><pre class="fssnip highlighted"><code lang="fsharp"><span class="c">// #################################################################################################</span>
<span class="c">// Author: Jeff Webster</span>
<span class="c">// Date:   2015/06/05</span>
<span class="c">// #################################################################################################</span>
<span class="c">//</span>
<span class="c">//</span>
<span class="c">// #################################################################################################</span>
<span class="c">// Generic Result Success / Fail handling -- much taken from http://fsharpforfunandprofit.com/rop</span>
<span class="c">// #################################################################################################</span>
<span class="c">// -------------------------------------------------------------------------------------------------</span>
<span class="c">// Typesafe way of capturing how program can fail</span>
<span class="c">// -------------------------------------------------------------------------------------------------</span>
<span class="k">type</span> <span class="t">Failures</span> <span class="o">=</span> 
   | <span onmouseout="hideTip(event, 'fs1', 1)" onmouseover="showTip(event, 'fs1', 1)" class="p">Debug</span>
   | <span onmouseout="hideTip(event, 'fs2', 2)" onmouseover="showTip(event, 'fs2', 2)" class="p">InvalidListSize</span>
   | <span onmouseout="hideTip(event, 'fs3', 3)" onmouseover="showTip(event, 'fs3', 3)" class="p">InvalidValue</span> <span class="k">of</span> <span onmouseout="hideTip(event, 'fs4', 4)" onmouseover="showTip(event, 'fs4', 4)" class="t">char</span>
   | <span onmouseout="hideTip(event, 'fs5', 5)" onmouseover="showTip(event, 'fs5', 5)" class="p">InvalidSuit</span> <span class="k">of</span> <span onmouseout="hideTip(event, 'fs4', 6)" onmouseover="showTip(event, 'fs4', 6)" class="t">char</span>
   | <span onmouseout="hideTip(event, 'fs6', 7)" onmouseover="showTip(event, 'fs6', 7)" class="p">InvalidCardSyntax</span>
   | <span onmouseout="hideTip(event, 'fs7', 8)" onmouseover="showTip(event, 'fs7', 8)" class="p">HandHasInvalidCards</span>
   | <span onmouseout="hideTip(event, 'fs8', 9)" onmouseover="showTip(event, 'fs8', 9)" class="p">HandHasDuplicates</span>
   | <span onmouseout="hideTip(event, 'fs9', 10)" onmouseover="showTip(event, 'fs9', 10)" class="p">MultiFail</span> <span class="k">of</span> <span onmouseout="hideTip(event, 'fs10', 11)" onmouseover="showTip(event, 'fs10', 11)" class="t">list</span><span class="o">&lt;</span><span onmouseout="hideTip(event, 'fs11', 12)" onmouseover="showTip(event, 'fs11', 12)" class="t">Failures</span><span class="o">&gt;</span>

<span class="c">// -------------------------------------------------------------------------------------------------</span>
<span class="c">// Convert Failures to a human-readable string</span>
<span class="c">// -------------------------------------------------------------------------------------------------</span>
<span class="k">let</span> <span class="k">rec</span> <span onmouseout="hideTip(event, 'fs12', 13)" onmouseover="showTip(event, 'fs12', 13)" class="f">explainFailure</span> <span onmouseout="hideTip(event, 'fs13', 14)" onmouseover="showTip(event, 'fs13', 14)" class="i">failType</span> <span class="o">=</span> 
   <span class="k">match</span> <span onmouseout="hideTip(event, 'fs13', 15)" onmouseover="showTip(event, 'fs13', 15)" class="i">failType</span> <span class="k">with</span>
   | <span onmouseout="hideTip(event, 'fs1', 16)" onmouseover="showTip(event, 'fs1', 16)" class="p">Debug</span> <span class="k">-&gt;</span> <span class="s">&quot;Debug Fail&quot;</span>
   | <span onmouseout="hideTip(event, 'fs2', 17)" onmouseover="showTip(event, 'fs2', 17)" class="p">InvalidListSize</span> <span class="k">-&gt;</span> <span class="s">&quot;Invalid List Size&quot;</span>
   | <span onmouseout="hideTip(event, 'fs3', 18)" onmouseover="showTip(event, 'fs3', 18)" class="p">InvalidValue</span> <span onmouseout="hideTip(event, 'fs14', 19)" onmouseover="showTip(event, 'fs14', 19)" class="i">v</span> <span class="k">-&gt;</span> <span onmouseout="hideTip(event, 'fs15', 20)" onmouseover="showTip(event, 'fs15', 20)" class="f">sprintf</span> <span class="s">&quot;Invalid Value of &#39;</span><span class="pf">%c</span><span class="s">&#39;&quot;</span> <span onmouseout="hideTip(event, 'fs14', 21)" onmouseover="showTip(event, 'fs14', 21)" class="i">v</span>
