   <span onmouseout="hideTip(event, 'fs141', 389)" onmouseover="showTip(event, 'fs141', 389)" class="f">printfn</span> <span class="s">&quot;</span><span class="pf">%s</span><span class="s"> </span><span class="pf">%s</span><span class="s">&quot;</span> <span onmouseout="hideTip(event, 'fs140', 390)" onmouseover="showTip(event, 'fs140', 390)" class="i">result</span> <span onmouseout="hideTip(event, 'fs137', 391)" onmouseover="showTip(event, 'fs137', 391)" class="i">desc</span>

