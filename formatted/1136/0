            <span class="k">let</span> <span onmouseout="hideTip(event, 'fs52', 151)" onmouseover="showTip(event, 'fs52', 151)" class="i">found</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs26', 152)" onmouseover="showTip(event, 'fs26', 152)" class="f">positionOnTarget</span> <span onmouseout="hideTip(event, 'fs22', 153)" onmouseover="showTip(event, 'fs22', 153)" class="i">position</span> <span onmouseout="hideTip(event, 'fs27', 154)" onmouseover="showTip(event, 'fs27', 154)" class="i">board</span> <span onmouseout="hideTip(event, 'fs28', 155)" onmouseover="showTip(event, 'fs28', 155)" class="i">target</span>

