                    <span onmouseout="hideTip(event, 'fs47', 174)" onmouseover="showTip(event, 'fs47', 174)" class="i">currentMass</span>


    <span class="c">// go through each board element and find masses starting at the</span>
    <span class="c">// the current position</span>
    <span class="c">// filter out any positions that found no masses</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs53', 175)" onmouseover="showTip(event, 'fs53', 175)" class="f">massFinder</span> <span onmouseout="hideTip(event, 'fs7', 176)" onmouseover="showTip(event, 'fs7', 176)" class="i">x</span> <span onmouseout="hideTip(event, 'fs13', 177)" onmouseover="showTip(event, 'fs13', 177)" class="i">y</span> <span onmouseout="hideTip(event, 'fs27', 178)" onmouseover="showTip(event, 'fs27', 178)" class="i">board</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs46', 179)" onmouseover="showTip(event, 'fs46', 179)" class="f">getLandMasses&#39;</span> (<span onmouseout="hideTip(event, 'fs7', 180)" onmouseover="showTip(event, 'fs7', 180)" class="i">x</span>, <span onmouseout="hideTip(event, 'fs13', 181)" onmouseover="showTip(event, 'fs13', 181)" class="i">y</span>) <span onmouseout="hideTip(event, 'fs27', 182)" onmouseover="showTip(event, 'fs27', 182)" class="i">board</span> []

