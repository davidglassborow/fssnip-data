  )
  
  <span class="c">// We get the stop event and only if relates to the current id trigger the stopNet event</span>
  <span class="k">let</span> <span onmouseout="hideTip(event, 'fs69', 145)" onmouseover="showTip(event, 'fs69', 145)" class="i">stopNet</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs70', 146)" onmouseover="showTip(event, 'fs70', 146)" class="t">Event</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs71', 147)" onmouseover="showTip(event, 'fs71', 147)" class="f">create</span><span class="o">&lt;</span><span onmouseout="hideTip(event, 'fs27', 148)" onmouseover="showTip(event, 'fs27', 148)" class="t">HttpEventArgs</span><span class="o">&gt;</span>(<span class="s">&quot;stopNet&quot;</span>)
