  <span class="c">// Net looping forever unless the stopNet event fires</span>
  <span class="k">let</span> <span onmouseout="hideTip(event, 'fs79', 179)" onmouseover="showTip(event, 'fs79', 179)" class="i">net</span> <span class="o">=</span> (<span onmouseout="hideTip(event, 'fs53', 180)" onmouseover="showTip(event, 'fs53', 180)" class="f">loopUntil</span> [|<span onmouseout="hideTip(event, 'fs69', 181)" onmouseover="showTip(event, 'fs69', 181)" class="i">stopNet</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs80', 182)" onmouseover="showTip(event, 'fs80', 182)" class="i">Publish</span>|] <span onmouseout="hideTip(event, 'fs66', 183)" onmouseover="showTip(event, 'fs66', 183)" class="i">doWork</span>)

  <span class="c">// Starts the net</span>
  <span onmouseout="hideTip(event, 'fs11', 184)" onmouseover="showTip(event, 'fs11', 184)" class="t">Expr</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs76', 185)" onmouseover="showTip(event, 'fs76', 185)" class="f">start</span> <span onmouseout="hideTip(event, 'fs77', 186)" onmouseover="showTip(event, 'fs77', 186)" class="t">Unchecked</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs78', 187)" onmouseover="showTip(event, 'fs78', 187)" class="i">defaultof</span><span class="o">&lt;</span>_<span class="o">&gt;</span> <span onmouseout="hideTip(event, 'fs57', 188)" onmouseover="showTip(event, 'fs57', 188)" class="i">orch</span> <span onmouseout="hideTip(event, 'fs79', 189)" onmouseover="showTip(event, 'fs79', 189)" class="i">net</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs74', 190)" onmouseover="showTip(event, 'fs74', 190)" class="f">ignore</span>
