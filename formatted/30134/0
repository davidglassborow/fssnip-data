                 <span class="o">|-&gt;</span> (<span class="k">fun</span> <span onmouseout="hideTip(event, 'fs48', 159)" onmouseover="showTip(event, 'fs48', 159)" class="i">arg</span> <span class="k">-&gt;</span> <span onmouseout="hideTip(event, 'fs48', 160)" onmouseover="showTip(event, 'fs48', 160)" class="i">arg</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs49', 161)" onmouseover="showTip(event, 'fs49', 161)" class="f">Result</span> <span class="o">&lt;-</span> <span onmouseout="hideTip(event, 'fs50', 162)" onmouseover="showTip(event, 'fs50', 162)" class="f">OK</span>(<span onmouseout="hideTip(event, 'fs65', 163)" onmouseover="showTip(event, 'fs65', 163)" class="f">sprintf</span> <span class="s">&quot;Job </span><span class="pf">%s</span><span class="s"> done&quot;</span> <span onmouseout="hideTip(event, 'fs62', 164)" onmouseover="showTip(event, 'fs62', 164)" class="i">id</span>)
