    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs121', 432)" onmouseover="showTip(event, 'fs121', 432)" class="i">binTree</span> <span class="o">=</span> (<span onmouseout="hideTip(event, 'fs97', 433)" onmouseover="showTip(event, 'fs97', 433)" class="t">Btd</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs12', 434)" onmouseover="showTip(event, 'fs12', 434)" class="i">empty</span>, <span onmouseout="hideTip(event, 'fs120', 435)" onmouseover="showTip(event, 'fs120', 435)" class="i">bins</span>) <span class="o">||&gt;</span> <span onmouseout="hideTip(event, 'fs111', 436)" onmouseover="showTip(event, 'fs111', 436)" class="t">List</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs112', 437)" onmouseover="showTip(event, 'fs112', 437)" class="f">fold</span> (<span class="k">fun</span> <span onmouseout="hideTip(event, 'fs87', 438)" onmouseover="showTip(event, 'fs87', 438)" class="i">t</span> <span onmouseout="hideTip(event, 'fs122', 439)" onmouseover="showTip(event, 'fs122', 439)" class="i">b</span> <span class="k">-&gt;</span> <span onmouseout="hideTip(event, 'fs87', 440)" onmouseover="showTip(event, 'fs87', 440)" class="i">t</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs97', 441)" onmouseover="showTip(event, 'fs97', 441)" class="t">Btd</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs99', 442)" onmouseover="showTip(event, 'fs99', 442)" class="f">add</span> (<span onmouseout="hideTip(event, 'fs122', 443)" onmouseover="showTip(event, 'fs122', 443)" class="i">b</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs76', 444)" onmouseover="showTip(event, 'fs76', 444)" class="i">Size</span>,<span onmouseout="hideTip(event, 'fs122', 445)" onmouseover="showTip(event, 'fs122', 445)" class="i">b</span>))
    
