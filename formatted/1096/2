    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs107', 385)" onmouseover="showTip(event, 'fs107', 385)" class="i">scheduled</span>,<span onmouseout="hideTip(event, 'fs108', 386)" onmouseover="showTip(event, 'fs108', 386)" class="i">capacities</span>,<span onmouseout="hideTip(event, 'fs109', 387)" onmouseover="showTip(event, 'fs109', 387)" class="i">unscheduled</span> <span class="o">=</span> 
