<span class="c">(* usage:</span>

<span class="c">let bins = [{Size=150; Id=&quot;a&quot;; Data=&quot;a&quot;}; {Size=235; Id=&quot;b&quot;; Data=&quot;b&quot;}; {Size=215;Id=&quot;c&quot;; Data=&quot;c&quot;}]</span>

<span class="c">let rng = System.Random()</span>
<span class="c">let items = [for i in 1..40 -&gt; {Size = rng.Next(1,30); Data=i.ToString()}]</span>

<span class="c">//sort items in descending order of size for best-fit decreasing order</span>
<span class="c">let sortedItems = items |&gt; List.sortBy (fun i -&gt; -i.Size)</span>

<span class="c">let schBest,_,leftOverItemsA = packBestFit bins sortedItems</span>
<span class="c">let schWorst,_,leftOverItemsB = packWorstFit bins sortedItems</span>

<span class="c">*)</span>
</code></pre></td>
