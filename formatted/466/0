                        <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs128', 415)" onmouseover="showTip(event, 'fs128', 415)" class="t">List</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs134', 416)" onmouseover="showTip(event, 'fs134', 416)" class="f">filter</span> (<span class="k">fun</span> (<span onmouseout="hideTip(event, 'fs160', 417)" onmouseover="showTip(event, 'fs160', 417)" class="i">poemNumber</span>, <span onmouseout="hideTip(event, 'fs161', 418)" onmouseover="showTip(event, 'fs161', 418)" class="i">linesPositions</span>) <span class="k">-&gt;</span> <span onmouseout="hideTip(event, 'fs93', 419)" onmouseover="showTip(event, 'fs93', 419)" class="f">not</span> (<span onmouseout="hideTip(event, 'fs128', 420)" onmouseover="showTip(event, 'fs128', 420)" class="t">List</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs162', 421)" onmouseover="showTip(event, 'fs162', 421)" class="f">isEmpty</span> <span onmouseout="hideTip(event, 'fs161', 422)" onmouseover="showTip(event, 'fs161', 422)" class="i">linesPositions</span>))

                (<span onmouseout="hideTip(event, 'fs141', 423)" onmouseover="showTip(event, 'fs141', 423)" class="i">token</span>, <span onmouseout="hideTip(event, 'fs143', 424)" onmouseover="showTip(event, 'fs143', 424)" class="f">mergePoems</span> <span onmouseout="hideTip(event, 'fs142', 425)" onmouseover="showTip(event, 'fs142', 425)" class="i">poems</span> <span onmouseout="hideTip(event, 'fs140', 426)" onmouseover="showTip(event, 'fs140', 426)" class="i">tokenIndex</span>)
