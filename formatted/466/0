            <span class="k">fun</span> (<span onmouseout="hideTip(event, 'fs141', 378)" onmouseover="showTip(event, 'fs141', 378)" class="i">token</span>, <span onmouseout="hideTip(event, 'fs142', 379)" onmouseover="showTip(event, 'fs142', 379)" class="i">poems</span>) <span class="k">-&gt;</span>
                <span class="k">let</span> <span onmouseout="hideTip(event, 'fs143', 380)" onmouseover="showTip(event, 'fs143', 380)" class="f">mergePoems</span> <span onmouseout="hideTip(event, 'fs144', 381)" onmouseover="showTip(event, 'fs144', 381)" class="i">currentPoems</span> <span onmouseout="hideTip(event, 'fs145', 382)" onmouseover="showTip(event, 'fs145', 382)" class="i">tokenPoems</span> <span class="o">=</span>
