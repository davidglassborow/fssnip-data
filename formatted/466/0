            <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs128', 343)" onmouseover="showTip(event, 'fs128', 343)" class="t">List</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs129', 344)" onmouseover="showTip(event, 'fs129', 344)" class="f">find</span> (<span class="k">fun</span>(<span onmouseout="hideTip(event, 'fs126', 345)" onmouseover="showTip(event, 'fs126', 345)" class="i">token</span>, <span onmouseout="hideTip(event, 'fs127', 346)" onmouseover="showTip(event, 'fs127', 346)" class="i">poems</span>) <span class="k">-&gt;</span> <span onmouseout="hideTip(event, 'fs126', 347)" onmouseover="showTip(event, 'fs126', 347)" class="i">token</span><span class="o">=</span><span onmouseout="hideTip(event, 'fs124', 348)" onmouseover="showTip(event, 'fs124', 348)" class="i">filterToken</span>)
    <span onmouseout="hideTip(event, 'fs127', 349)" onmouseover="showTip(event, 'fs127', 349)" class="i">poems</span>
        <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs128', 350)" onmouseover="showTip(event, 'fs128', 350)" class="t">List</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs130', 351)" onmouseover="showTip(event, 'fs130', 351)" class="f">map</span>
        (
            <span class="k">fun</span> (<span onmouseout="hideTip(event, 'fs131', 352)" onmouseover="showTip(event, 'fs131', 352)" class="i">poemNumber</span>, <span onmouseout="hideTip(event, 'fs132', 353)" onmouseover="showTip(event, 'fs132', 353)" class="i">linesPositions</span>) <span class="k">-&gt;</span>
