<span class="c">//intersect current index with token index</span>
<span class="c">//we want to only keep tokens and poems which are present in the token index (which is a subtree of full index, see above)</span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs138', 372)" onmouseover="showTip(event, 'fs138', 372)" class="f">intersectIndex</span> <span onmouseout="hideTip(event, 'fs139', 373)" onmouseover="showTip(event, 'fs139', 373)" class="i">currentIndex</span> <span onmouseout="hideTip(event, 'fs140', 374)" onmouseover="showTip(event, 'fs140', 374)" class="i">tokenIndex</span> <span class="o">=</span>
