                        <span class="k">yield</span> (<span onmouseout="hideTip(event, 'fs125', 477)" onmouseover="showTip(event, 'fs125', 477)" class="i">t</span>, <span onmouseout="hideTip(event, 'fs126', 478)" onmouseover="showTip(event, 'fs126', 478)" class="i">u</span>)
    }

