<span class="c">/// Takes a root note and chord type and returns the actual notes of the chord:</span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs104', 381)" onmouseover="showTip(event, 'fs104', 381)" class="f">chordOf</span> (<span onmouseout="hideTip(event, 'fs101', 382)" onmouseover="showTip(event, 'fs101', 382)" class="i">rootNote</span> <span class="o">:</span> <span onmouseout="hideTip(event, 'fs12', 383)" onmouseover="showTip(event, 'fs12', 383)" class="t">TNote</span>) (<span onmouseout="hideTip(event, 'fs105', 384)" onmouseover="showTip(event, 'fs105', 384)" class="i">chordType</span> <span class="o">:</span> <span onmouseout="hideTip(event, 'fs20', 385)" onmouseover="showTip(event, 'fs20', 385)" class="t">TChordType</span>) <span class="o">=</span>
    <span onmouseout="hideTip(event, 'fs105', 386)" onmouseover="showTip(event, 'fs105', 386)" class="i">chordType</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs21', 387)" onmouseover="showTip(event, 'fs21', 387)" class="i">chordIndexes</span>
    <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs68', 388)" onmouseover="showTip(event, 'fs68', 388)" class="t">Array</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs106', 389)" onmouseover="showTip(event, 'fs106', 389)" class="f">map</span> (<span class="k">fun</span> <span onmouseout="hideTip(event, 'fs78', 390)" onmouseover="showTip(event, 'fs78', 390)" class="i">index</span> <span class="k">-&gt;</span> <span onmouseout="hideTip(event, 'fs91', 391)" onmouseover="showTip(event, 'fs91', 391)" class="f">addSemitones</span> <span onmouseout="hideTip(event, 'fs101', 392)" onmouseover="showTip(event, 'fs101', 392)" class="i">rootNote</span> <span onmouseout="hideTip(event, 'fs78', 393)" onmouseover="showTip(event, 'fs78', 393)" class="i">index</span> (<span onmouseout="hideTip(event, 'fs100', 394)" onmouseover="showTip(event, 'fs100', 394)" class="f">rootNoteToEnharmonicPreference</span> <span onmouseout="hideTip(event, 'fs101', 395)" onmouseover="showTip(event, 'fs101', 395)" class="i">rootNote</span> <span onmouseout="hideTip(event, 'fs105', 396)" onmouseover="showTip(event, 'fs105', 396)" class="i">chordType</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs22', 397)" onmouseover="showTip(event, 'fs22', 397)" class="i">scaleType</span>) )

<span class="c">/// List all the notes you could play on a given string between a lowest and highest fret</span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs107', 398)" onmouseover="showTip(event, 'fs107', 398)" class="f">notesBetween</span> (<span onmouseout="hideTip(event, 'fs95', 399)" onmouseover="showTip(event, 'fs95', 399)" class="i">instrument</span> <span class="o">:</span> <span onmouseout="hideTip(event, 'fs38', 400)" onmouseover="showTip(event, 'fs38', 400)" class="t">TInstrument</span>) (<span onmouseout="hideTip(event, 'fs108', 401)" onmouseover="showTip(event, 'fs108', 401)" class="i">stringIndex</span> <span class="o">:</span> <span onmouseout="hideTip(event, 'fs11', 402)" onmouseover="showTip(event, 'fs11', 402)" class="t">int</span>) (<span onmouseout="hideTip(event, 'fs109', 403)" onmouseover="showTip(event, 'fs109', 403)" class="i">lowestFret</span> <span class="o">:</span> <span onmouseout="hideTip(event, 'fs11', 404)" onmouseover="showTip(event, 'fs11', 404)" class="t">int</span>) (<span onmouseout="hideTip(event, 'fs110', 405)" onmouseover="showTip(event, 'fs110', 405)" class="i">highestFret</span> <span class="o">:</span> <span onmouseout="hideTip(event, 'fs11', 406)" onmouseover="showTip(event, 'fs11', 406)" class="t">int</span>) <span class="o">:</span> <span onmouseout="hideTip(event, 'fs111', 407)" onmouseover="showTip(event, 'fs111', 407)" class="t">seq</span><span class="o">&lt;</span><span onmouseout="hideTip(event, 'fs35', 408)" onmouseover="showTip(event, 'fs35', 408)" class="t">TFrettedNote</span><span class="o">&gt;</span> <span class="o">=</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs112', 409)" onmouseover="showTip(event, 'fs112', 409)" class="i">frettedNotes</span> <span class="o">=</span> 
        <span onmouseout="hideTip(event, 'fs111', 410)" onmouseover="showTip(event, 'fs111', 410)" class="i">seq</span> {
            <span class="k">for</span> <span onmouseout="hideTip(event, 'fs113', 411)" onmouseover="showTip(event, 'fs113', 411)" class="i">fret</span> <span class="k">in</span> <span onmouseout="hideTip(event, 'fs109', 412)" onmouseover="showTip(event, 'fs109', 412)" class="i">lowestFret</span><span class="o">..</span><span onmouseout="hideTip(event, 'fs110', 413)" onmouseover="showTip(event, 'fs110', 413)" class="i">highestFret</span> <span class="k">do</span> 
