    { <span class="i">Re</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs9', 83)" onmouseover="showTip(event, 'fs9', 83)" class="i">z</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs1', 84)" onmouseover="showTip(event, 'fs1', 84)" class="i">Re</span>; 
      <span class="i">Im</span> <span class="o">=</span> <span class="o">-</span><span onmouseout="hideTip(event, 'fs9', 85)" onmouseover="showTip(event, 'fs9', 85)" class="i">z</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs3', 86)" onmouseover="showTip(event, 'fs3', 86)" class="i">Im</span> };;

<span class="c">// ... and the modulus (absolute value)</span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs15', 87)" onmouseover="showTip(event, 'fs15', 87)" class="f">abs</span> <span onmouseout="hideTip(event, 'fs9', 88)" onmouseover="showTip(event, 'fs9', 88)" class="i">z</span> <span class="o">=</span>
    <span onmouseout="hideTip(event, 'fs16', 89)" onmouseover="showTip(event, 'fs16', 89)" class="f">sqrt</span> (<span onmouseout="hideTip(event, 'fs9', 90)" onmouseover="showTip(event, 'fs9', 90)" class="i">z</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs1', 91)" onmouseover="showTip(event, 'fs1', 91)" class="i">Re</span> <span class="o">*</span> <span onmouseout="hideTip(event, 'fs9', 92)" onmouseover="showTip(event, 'fs9', 92)" class="i">z</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs1', 93)" onmouseover="showTip(event, 'fs1', 93)" class="i">Re</span> <span class="o">+</span> <span onmouseout="hideTip(event, 'fs9', 94)" onmouseover="showTip(event, 'fs9', 94)" class="i">z</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs3', 95)" onmouseover="showTip(event, 'fs3', 95)" class="i">Im</span> <span class="o">*</span> <span onmouseout="hideTip(event, 'fs9', 96)" onmouseover="showTip(event, 'fs9', 96)" class="i">z</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs3', 97)" onmouseover="showTip(event, 'fs3', 97)" class="i">Im</span>);;

<span class="c">// ... and the argument (actually this is the principal value of the argument (Arg)</span>
