    <span class="i">Transfer</span><span class="o">=</span><span onmouseout="hideTip(event, 'fs88', 954)" onmouseover="showTip(event, 'fs88', 954)" class="p">None</span>}
   ]

<span class="k">let</span> <span onmouseout="hideTip(event, 'fs165', 955)" onmouseover="showTip(event, 'fs165', 955)" class="f">names</span> () <span class="o">=</span>
   <span class="k">let</span> <span onmouseout="hideTip(event, 'fs166', 956)" onmouseover="showTip(event, 'fs166', 956)" class="i">names</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs64', 957)" onmouseover="showTip(event, 'fs64', 957)" class="i">seq</span> [<span class="s">&quot;Billy&quot;</span>; <span class="s">&quot;Bob&quot;</span>; <span class="s">&quot;Thornton&quot;</span>]
   <span class="k">let</span> <span onmouseout="hideTip(event, 'fs167', 958)" onmouseover="showTip(event, 'fs167', 958)" class="i">e</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs166', 959)" onmouseover="showTip(event, 'fs166', 959)" class="i">names</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs168', 960)" onmouseover="showTip(event, 'fs168', 960)" class="f">GetEnumerator</span>()
   <span class="k">fun</span> () <span class="k">-&gt;</span> <span class="k">if</span> <span onmouseout="hideTip(event, 'fs167', 961)" onmouseover="showTip(event, 'fs167', 961)" class="i">e</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs169', 962)" onmouseover="showTip(event, 'fs169', 962)" class="f">MoveNext</span>() <span class="k">then</span> <span onmouseout="hideTip(event, 'fs167', 963)" onmouseover="showTip(event, 'fs167', 963)" class="i">e</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs170', 964)" onmouseover="showTip(event, 'fs170', 964)" class="i">Current</span> <span class="k">else</span> <span class="s">&quot;&quot;</span>
<span onmouseout="hideTip(event, 'fs164', 965)" onmouseover="showTip(event, 'fs164', 965)" class="i">loop</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs94', 966)" onmouseover="showTip(event, 'fs94', 966)" class="f">run</span> (<span onmouseout="hideTip(event, 'fs165', 967)" onmouseover="showTip(event, 'fs165', 967)" class="f">names</span> ()) <span onmouseout="hideTip(event, 'fs161', 968)" onmouseover="showTip(event, 'fs161', 968)" class="f">output</span>
<span class="c">// &gt; This program will ask you for personal names</span>
<span class="c">// &gt; until you press return without giving it one</span>
<span class="c">// &gt; Name 1: Billy</span>
<span class="c">// &gt; Name 2: Bob</span>
