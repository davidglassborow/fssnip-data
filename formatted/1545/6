 <span onmouseout="hideTip(event, 'fs218', 1230)" onmouseover="showTip(event, 'fs218', 1230)" class="t">Line</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs224', 1231)" onmouseover="showTip(event, 'fs224', 1231)" class="f">Of</span>( <span class="s">&quot;OUTPUT&quot;</span> <span class="o">=.</span> <span onmouseout="hideTip(event, 'fs27', 1232)" onmouseover="showTip(event, 'fs27', 1232)" class="p">Concat</span> [<span onmouseout="hideTip(event, 'fs196', 1233)" onmouseover="showTip(event, 'fs196', 1233)" class="f">S</span><span class="s">&quot;Thank you, &quot;</span>; <span onmouseout="hideTip(event, 'fs199', 1234)" onmouseover="showTip(event, 'fs199', 1234)" class="f">V</span><span class="s">&quot;Username&quot;</span>])]
