   <span onmouseout="hideTip(event, 'fs218', 1645)" onmouseover="showTip(event, 'fs218', 1645)" class="t">Line</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs224', 1646)" onmouseover="showTip(event, 'fs224', 1646)" class="f">Of</span>(<span class="s">&quot;WCOUNT&quot;</span> <span class="o">=.</span> <span onmouseout="hideTip(event, 'fs31', 1647)" onmouseover="showTip(event, 'fs31', 1647)" class="p">NewTable</span>)
