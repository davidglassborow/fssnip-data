<td class="snippet"><pre class="fssnip highlighted"><code lang="fsharp"><span class="c">(*</span>
<span class="c">          OUTPUT = &quot;This program will ask you for personal names&quot;</span>
<span class="c">          OUTPUT = &quot;until you press return without giving it one&quot;</span>
<span class="c">          NameCount = 0                                            :(GETINPUT)</span>
<span class="c">AGAIN     NameCount = NameCount + 1</span>
<span class="c">          OUTPUT = &quot;Name &quot; NameCount &quot;: &quot; PersonalName</span>
<span class="c">GETINPUT  OUTPUT = &quot;Please give me name &quot; NameCount + 1 </span>
<span class="c">          PersonalName = INPUT</span>
<span class="c">          PersonalName LEN(1)                                      :S(AGAIN)</span>
<span class="c">          OUTPUT = &quot;Finished. &quot; NameCount &quot; names requested.&quot;</span>
<span class="c">END</span>
<span class="c">*)</span>

<span class="k">let</span> <span onmouseout="hideTip(event, 'fs226', 1293)" onmouseover="showTip(event, 'fs226', 1293)" class="i">loop</span> <span class="o">=</span>
   [
   <span onmouseout="hideTip(event, 'fs218', 1294)" onmouseover="showTip(event, 'fs218', 1294)" class="t">Line</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs224', 1295)" onmouseover="showTip(event, 'fs224', 1295)" class="f">Of</span>(<span class="s">&quot;OUTPUT&quot;</span> <span class="o">=.</span> <span onmouseout="hideTip(event, 'fs196', 1296)" onmouseover="showTip(event, 'fs196', 1296)" class="f">S</span><span class="s">&quot;This program will ask you for personal names&quot;</span>)
   <span onmouseout="hideTip(event, 'fs218', 1297)" onmouseover="showTip(event, 'fs218', 1297)" class="t">Line</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs224', 1298)" onmouseover="showTip(event, 'fs224', 1298)" class="f">Of</span>(<span class="s">&quot;OUTPUT&quot;</span> <span class="o">=.</span> <span onmouseout="hideTip(event, 'fs196', 1299)" onmouseover="showTip(event, 'fs196', 1299)" class="f">S</span><span class="s">&quot;until you press return without giving it one&quot;</span>)
   <span onmouseout="hideTip(event, 'fs218', 1300)" onmouseover="showTip(event, 'fs218', 1300)" class="t">Line</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs224', 1301)" onmouseover="showTip(event, 'fs224', 1301)" class="f">Of</span>(<span class="s">&quot;NameCount&quot;</span> <span class="o">=.</span> <span onmouseout="hideTip(event, 'fs197', 1302)" onmouseover="showTip(event, 'fs197', 1302)" class="f">I</span> <span class="n">0</span>, <span onmouseout="hideTip(event, 'fs38', 1303)" onmouseover="showTip(event, 'fs38', 1303)" class="i">transfer</span><span class="o">=</span><span onmouseout="hideTip(event, 'fs39', 1304)" onmouseover="showTip(event, 'fs39', 1304)" class="p">Goto</span> <span class="s">&quot;GETINPUT&quot;</span>)
   <span onmouseout="hideTip(event, 'fs218', 1305)" onmouseover="showTip(event, 'fs218', 1305)" class="t">Line</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs224', 1306)" onmouseover="showTip(event, 'fs224', 1306)" class="f">Of</span>(<span class="s">&quot;NameCount&quot;</span> <span class="o">=.</span> <span onmouseout="hideTip(event, 'fs199', 1307)" onmouseover="showTip(event, 'fs199', 1307)" class="f">V</span><span class="s">&quot;NameCount&quot;</span> <span class="o">+.</span> <span onmouseout="hideTip(event, 'fs197', 1308)" onmouseover="showTip(event, 'fs197', 1308)" class="f">I</span> <span class="n">1</span>, <span onmouseout="hideTip(event, 'fs40', 1309)" onmouseover="showTip(event, 'fs40', 1309)" class="i">label</span><span class="o">=</span><span class="s">&quot;AGAIN&quot;</span>)
