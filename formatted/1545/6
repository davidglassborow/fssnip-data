   <span onmouseout="hideTip(event, 'fs218', 1317)" onmouseover="showTip(event, 'fs218', 1317)" class="t">Line</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs224', 1318)" onmouseover="showTip(event, 'fs224', 1318)" class="f">Of</span>(<span class="s">&quot;PersonalName&quot;</span> <span class="o">=.</span> <span onmouseout="hideTip(event, 'fs199', 1319)" onmouseover="showTip(event, 'fs199', 1319)" class="f">V</span><span class="s">&quot;INPUT&quot;</span>, <span onmouseout="hideTip(event, 'fs40', 1320)" onmouseover="showTip(event, 'fs40', 1320)" class="i">label</span><span class="o">=</span><span class="s">&quot;GETINPUT&quot;</span>)
   <span onmouseout="hideTip(event, 'fs218', 1321)" onmouseover="showTip(event, 'fs218', 1321)" class="t">Line</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs224', 1322)" onmouseover="showTip(event, 'fs224', 1322)" class="f">Of</span>(<span onmouseout="hideTip(event, 'fs199', 1323)" onmouseover="showTip(event, 'fs199', 1323)" class="f">V</span><span class="s">&quot;PersonalName&quot;</span> <span class="o">=?</span> <span onmouseout="hideTip(event, 'fs209', 1324)" onmouseover="showTip(event, 'fs209', 1324)" class="f">LEN</span>(<span onmouseout="hideTip(event, 'fs197', 1325)" onmouseover="showTip(event, 'fs197', 1325)" class="f">I</span> <span class="n">1</span>), <span onmouseout="hideTip(event, 'fs38', 1326)" onmouseover="showTip(event, 'fs38', 1326)" class="i">transfer</span><span class="o">=</span><span onmouseout="hideTip(event, 'fs42', 1327)" onmouseover="showTip(event, 'fs42', 1327)" class="p">OnSuccess</span> <span class="s">&quot;AGAIN&quot;</span>)
   <span onmouseout="hideTip(event, 'fs218', 1328)" onmouseover="showTip(event, 'fs218', 1328)" class="t">Line</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs224', 1329)" onmouseover="showTip(event, 'fs224', 1329)" class="f">Of</span>(<span class="s">&quot;OUTPUT&quot;</span> <span class="o">=.</span> <span onmouseout="hideTip(event, 'fs27', 1330)" onmouseover="showTip(event, 'fs27', 1330)" class="p">Concat</span> [<span onmouseout="hideTip(event, 'fs196', 1331)" onmouseover="showTip(event, 'fs196', 1331)" class="f">S</span><span class="s">&quot;Finished. &quot;</span>;<span onmouseout="hideTip(event, 'fs199', 1332)" onmouseover="showTip(event, 'fs199', 1332)" class="f">V</span><span class="s">&quot;NameCount&quot;</span>;<span onmouseout="hideTip(event, 'fs196', 1333)" onmouseover="showTip(event, 'fs196', 1333)" class="f">S</span><span class="s">&quot; names requested.&quot;</span>] )
   ]

<span class="k">let</span> <span onmouseout="hideTip(event, 'fs227', 1334)" onmouseover="showTip(event, 'fs227', 1334)" class="i">names</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs80', 1335)" onmouseover="showTip(event, 'fs80', 1335)" class="i">seq</span> [<span class="s">&quot;Billy&quot;</span>; <span class="s">&quot;Bob&quot;</span>; <span class="s">&quot;Thornton&quot;</span>]
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs228', 1336)" onmouseover="showTip(event, 'fs228', 1336)" class="f">successive</span> (<span onmouseout="hideTip(event, 'fs229', 1337)" onmouseover="showTip(event, 'fs229', 1337)" class="i">inputs</span><span class="o">:</span><span onmouseout="hideTip(event, 'fs1', 1338)" onmouseover="showTip(event, 'fs1', 1338)" class="t">string</span> <span onmouseout="hideTip(event, 'fs80', 1339)" onmouseover="showTip(event, 'fs80', 1339)" class="t">seq</span>) <span class="o">=</span>
   <span class="k">let</span> <span onmouseout="hideTip(event, 'fs230', 1340)" onmouseover="showTip(event, 'fs230', 1340)" class="i">e</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs229', 1341)" onmouseover="showTip(event, 'fs229', 1341)" class="i">inputs</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs231', 1342)" onmouseover="showTip(event, 'fs231', 1342)" class="f">GetEnumerator</span>()
   <span class="k">fun</span> () <span class="k">-&gt;</span> <span class="k">if</span> <span onmouseout="hideTip(event, 'fs230', 1343)" onmouseover="showTip(event, 'fs230', 1343)" class="i">e</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs232', 1344)" onmouseover="showTip(event, 'fs232', 1344)" class="f">MoveNext</span>() <span class="k">then</span> <span onmouseout="hideTip(event, 'fs230', 1345)" onmouseover="showTip(event, 'fs230', 1345)" class="i">e</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs233', 1346)" onmouseover="showTip(event, 'fs233', 1346)" class="i">Current</span> <span class="k">else</span> <span class="s">&quot;&quot;</span>
<span onmouseout="hideTip(event, 'fs226', 1347)" onmouseover="showTip(event, 'fs226', 1347)" class="i">loop</span> <span class="o">|&gt;</span> <span onmouseout="hideTip(event, 'fs183', 1348)" onmouseover="showTip(event, 'fs183', 1348)" class="f">run</span> { <span onmouseout="hideTip(event, 'fs222', 1349)" onmouseover="showTip(event, 'fs222', 1349)" class="i">machine</span> <span class="k">with</span> <span class="i">Input</span><span class="o">=</span><span onmouseout="hideTip(event, 'fs228', 1350)" onmouseover="showTip(event, 'fs228', 1350)" class="f">successive</span> <span onmouseout="hideTip(event, 'fs227', 1351)" onmouseover="showTip(event, 'fs227', 1351)" class="i">names</span> }
<span class="c">// &gt; This program will ask you for personal names</span>
<span class="c">// &gt; until you press return without giving it one</span>
<span class="c">// &gt; Name 1: Billy</span>
<span class="c">// &gt; Name 2: Bob</span>
<span class="c">// &gt; Name 3: Thornton</span>
<span class="c">// &gt; Finished. 3 names requested.</span>
