  <span class="c">//Config</span>
  <span class="k">let</span> <span onmouseout="hideTip(event, 'fs52', 93)" onmouseover="showTip(event, 'fs52', 93)" class="i">root</span>,<span onmouseout="hideTip(event, 'fs53', 94)" onmouseover="showTip(event, 'fs53', 94)" class="i">host</span> <span class="o">=</span> <span class="s">@&quot;C:\RestForFree&quot;</span>,<span class="s">&quot;http://localhost:8888/&quot;</span>

  <span class="c">//.NET init stuff</span>
  <span class="k">let</span> <span onmouseout="hideTip(event, 'fs54', 95)" onmouseover="showTip(event, 'fs54', 95)" class="i">initListener</span> <span class="o">=</span>
