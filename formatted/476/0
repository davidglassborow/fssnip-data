    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs165', 778)" onmouseover="showTip(event, 'fs165', 778)" class="i">starts</span> <span class="o">=</span> [<span onmouseout="hideTip(event, 'fs163', 779)" onmouseover="showTip(event, 'fs163', 779)" class="i">a</span> <span class="o">..</span> <span onmouseout="hideTip(event, 'fs133', 780)" onmouseover="showTip(event, 'fs133', 780)" class="i">step</span> <span class="o">..</span> <span onmouseout="hideTip(event, 'fs164', 781)" onmouseover="showTip(event, 'fs164', 781)" class="i">b</span><span class="o">-</span><span onmouseout="hideTip(event, 'fs133', 782)" onmouseover="showTip(event, 'fs133', 782)" class="i">step</span>] 
