<span class="c">// Problem 8.3 (c)</span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs181', 838)" onmouseover="showTip(event, 'fs181', 838)" class="f">p</span> <span onmouseout="hideTip(event, 'fs154', 839)" onmouseover="showTip(event, 'fs154', 839)" class="i">t</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs154', 840)" onmouseover="showTip(event, 'fs154', 840)" class="i">t</span> <span class="o">+</span> (<span onmouseout="hideTip(event, 'fs154', 841)" onmouseover="showTip(event, 'fs154', 841)" class="i">t</span><span class="o">**</span><span class="n">2.0</span>) <span class="o">*</span> <span onmouseout="hideTip(event, 'fs62', 842)" onmouseover="showTip(event, 'fs62', 842)" class="i">i</span>
<span class="k">let</span> <span onmouseout="hideTip(event, 'fs182', 843)" onmouseover="showTip(event, 'fs182', 843)" class="f">f</span> <span onmouseout="hideTip(event, 'fs11', 844)" onmouseover="showTip(event, 'fs11', 844)" class="i">z</span> <span class="o">=</span> {<span class="i">Re</span> <span class="o">=</span> <span onmouseout="hideTip(event, 'fs11', 845)" onmouseover="showTip(event, 'fs11', 845)" class="i">z</span><span class="o">.</span><span onmouseout="hideTip(event, 'fs1', 846)" onmouseover="showTip(event, 'fs1', 846)" class="i">Re</span>; <span class="i">Im</span> <span class="o">=</span> <span class="n">0.0</span>}

