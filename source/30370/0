let newScore = function |"0", b     ->"15", b   |"15", b  ->"30", b|"30",b->"40",b
                                           |"40","A"->"40","40"|"40", b  ->"A" , b |_, b ->"Win",b

List.fold(fun (a,b)->function |2->newScore(b,a)  |_->newScore(a,b)) ("0","0") [1;2;1;1;1;2;1;1;1;1]

